  RFC1179 [label="RFC1179
Line printer daemon protocol
1990", shape=ellipse, fillcolor="#e8f0ff", tooltip="This RFC describes an existing print server protocol widely used on the Internet for communicating between line printer daemons (both clients and servers).  This memo is for informational purposes only, and does not specify an Internet standard.  Please refer to the current edition of the IAB Official Protocol Standards for the standardization state and status of this protocol.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

