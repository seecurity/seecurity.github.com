  RFC5665 [label="RFC5665
IANA Considerations for Remote Procedure
Call (RPC) Network Identifiers and
Universal Address Formats
2010", shape=ellipse, URL="RFC5665.html", target="_top", tooltip="This document lists IANA Considerations for Remote Procedure Call (RPC) Network Identifiers (netids) and RPC Universal Network Addresses (uaddrs).  This document updates, but does not replace, RFC 1833. [STANDARDS-TRACK]"];
  RFC5665 -> updated_by;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
