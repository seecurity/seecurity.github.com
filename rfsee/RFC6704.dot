  RFC6704 [label="RFC6704
Forcerenew Nonce Authentication
2012", shape=ellipse, fillcolor="#e8f0ff", tooltip="Dynamic Host Configuration Protocol (DHCP) FORCERENEW allows for the reconfiguration of a single host by forcing the DHCP client into a Renew state on a trigger from the DHCP server.  In the Forcerenew Nonce Authentication protocol, the server sends a nonce to the client in the initial DHCP ACK that is used for subsequent validation of a FORCERENEW message.  This document updates RFC 3203. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC6704;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC6704 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC6704 -> updates;
  RFC3203 [label="RFC3203
DHCP reconfigure extension
2001", shape=ellipse, URL="RFC3203.html", target="_top", tooltip="This document defines extensions to DHCP (Dynamic Host Configuration Protocol) to allow dynamic reconfiguration of a single host triggered by the DHCP server (e.g., a new IP address and/or local configuration parameters). [STANDARDS-TRACK]"];
  updates -> RFC3203;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC6704;

