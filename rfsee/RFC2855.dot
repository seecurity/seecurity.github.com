digraph Flow {
  rankdir=LR;
  splines=true;
  concentrate=true;   // merge parallel edges / reduce clutter
  nodesep=0.25;       // horizontal/vertical separation between nodes
  ranksep=0.35;       // separation between ranks (columns in LR)
  RFC2855 [label="RFC2855
DHCP for IEEE 1394
2000", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo describes specific usage of some fields of DHCP (Dynamic Host Configuration Protocol) messages.  IEEE Std 1394-1995 is a standard for a High Performance Serial Bus.  Since 1394 uses a different link-layer addressing method than conventional IEEE802/Ethernet, the usage of some fields must be clarified to achieve interoperability. [STANDARDS-TRACK]", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2855;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2855 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2855 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2855;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2855 -> cites;
  RFC2734 [label="RFC2734
IPv4 over IEEE 1394
1999", shape=ellipse, URL="RFC2734.html", target="_top", tooltip="This document specifies how to use IEEE Std 1394-1995, Standard for a High Performance Serial Bus (and its supplements), for the transport of Internet Protocol Version 4 (IPv4) datagrams; it defines the necessary methods, data structures and codes for that purpose. [STANDARDS-TRACK]"];
  cites -> RFC2734;
  RFC2131 [label="RFC2131
Dynamic Host Configuration Protocol
1997", shape=ellipse, URL="RFC2131.html", target="_top", tooltip="The Dynamic Host Configuration Protocol (DHCP) provides a framework for passing configuration information to hosts on a TCPIP network.  DHCP is based on the Bootstrap Protocol (BOOTP), adding the capability of automatic allocation of reusable network addresses and additional configuration options. [STANDARDS-TRACK]"];
  cites -> RFC2131;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2132 [label="RFC2132
DHCP Options and BOOTP Vendor Extensions
1997", shape=ellipse, URL="RFC2132.html", target="_top", tooltip="This document specifies the current set of DHCP options.  Future options will be specified in separate RFCs.  The current list of valid options is also available in ftp://ftp.isi.edu/in-notes/iana/assignments. [STANDARDS-TRACK]"];
  cites -> RFC2132;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2855;
  RFC4390 [label="RFC4390
Dynamic Host Configuration Protocol
(DHCP) over InfiniBand
2006", shape=ellipse, URL="RFC4390.html", target="_top", tooltip="IP over Infiniband (IPoIB) link-layer address is 20 octets long.  This is larger than the 16 octets reserved for the hardware address in a Dynamic Host Configuration Protocol/Bootstrap Protocol (DHCP/BOOTP) message.  The above inequality imposes restrictions on the use of the DHCP message fields when used over an IPoIB network.  This document describes the use of DHCP message fields when implementing DHCP over IPoIB. [STANDARDS-TRACK]"];
  RFC4390 -> rev_cites;
}
