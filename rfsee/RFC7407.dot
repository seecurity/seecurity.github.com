  RFC7407 [label="RFC7407
A YANG Data Model for SNMP Configuration
2014", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines a collection of YANG definitions for configuring SNMP engines.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC7407;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
