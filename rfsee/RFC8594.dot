  RFC8594 [label="RFC8594
The Sunset HTTP Header Field
2019", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification defines the Sunset HTTP response header field, which indicates that a URI is likely to become unresponsive at a specified point in the future.  It also defines a sunset link relation type that allows linking to resources providing information about an upcoming resource or service sunset.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8594;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8594 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8594 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC8594;

