  RFC2790 [label="RFC2790
Host Resources MIB
2000", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo obsoletes RFC 1514, the Host Resources MIB.  This memo extends that specification by clarifying changes based on implementation and deployment experience and documenting the Host Resources MIB in SMIv2 format while remaining semantically identical to the existing SMIv1-based MIB. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2790;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2790 -> obs;
  RFC1514 [label="RFC1514
Host Resources MIB
1993", shape=ellipse, URL="RFC1514.html", target="_top", tooltip="This memo defines a MIB for use with managing host systems. [STANDARDS-TRACK]"];
  obs -> RFC1514;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2790 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
