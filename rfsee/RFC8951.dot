  RFC8951 [label="RFC8951
Clarification of Enrollment over Secure
Transport (EST): Transfer Encodings and
ASN.1
2020", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document updates RFC 7030: Enrollment over Secure Transport to resolve some errata that were reported and that have proven to cause interoperability issues when RFC 7030 was extended. This document deprecates the specification of Content-Transfer-Encoding headers for Enrollment over Secure Transport (EST) endpoints. This document fixes some syntactical errors in ASN.1 that were present.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8951;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8951 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8951 -> updates;
  RFC7030 [label="RFC7030
Enrollment over Secure Transport
2013", shape=ellipse, URL="RFC7030.html", target="_top", tooltip="This document profiles certificate enrollment for clients using Certificate Management over CMS (CMC) messages over a secure transport.  This profile, called Enrollment over Secure Transport (EST), describes a simple, yet functional, certificate management protocol targeting Public Key Infrastructure (PKI) clients that need to acquire client certificates and associated Certification Authority (CA) certificates.  It also supports client-generated public/private key pairs as well as key pairs generated by the CA."];
  updates -> RFC7030;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC8951;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC8951 -> cites;
  RFC7030 [label="RFC7030
Enrollment over Secure Transport
2013", shape=ellipse, URL="RFC7030.html", target="_top", tooltip="This document profiles certificate enrollment for clients using Certificate Management over CMS (CMC) messages over a secure transport.  This profile, called Enrollment over Secure Transport (EST), describes a simple, yet functional, certificate management protocol targeting Public Key Infrastructure (PKI) clients that need to acquire client certificates and associated Certification Authority (CA) certificates.  It also supports client-generated public/private key pairs as well as key pairs generated by the CA."];
  cites -> RFC7030;
