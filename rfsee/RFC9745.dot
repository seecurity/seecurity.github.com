  RFC9745 [label="RFC9745
The Deprecation HTTP Response Header
Field
2025", shape=ellipse, fillcolor="#e8f0ff", tooltip="The Deprecation HTTP response header field is used to signal to consumers of a resource (identified by a URI) that the resource will be or has been deprecated.  Additionally, the deprecation link relation can be used to link to a resource that provides further information about planned or existing deprecation.  It may also provide ways in which client application developers can best manage deprecation.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC9745;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC9745 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC9745 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC9745;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC9745 -> cites;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC8174 [label="RFC8174
Ambiguity of Uppercase vs Lowercase in
RFC 2119 Key Words
2017", shape=ellipse, URL="RFC8174.html", target="_top", tooltip="RFC 2119 specifies common key words that may be used in protocol specifications.  This document aims to reduce the ambiguity by clarifying that only UPPERCASE usage of the key words have the defined special meanings."];
  cites -> RFC8174;
  RFC9651 [label="RFC9651
Structured Field Values for HTTP
2024", shape=ellipse, URL="RFC9651.html", target="_top", tooltip="This document describes a set of data types and associated algorithms that are intended to make it easier and safer to define and handle HTTP header and trailer fields, known as Structured Fields, Structured Headers, or Structured Trailers. It is intended for use by specifications of new HTTP fields. This document obsoletes RFC 8941."];
  cites -> RFC9651;
  RFC8594 [label="RFC8594
The Sunset HTTP Header Field
2019", shape=ellipse, URL="RFC8594.html", target="_top", tooltip="This specification defines the Sunset HTTP response header field, which indicates that a URI is likely to become unresponsive at a specified point in the future.  It also defines a sunset link relation type that allows linking to resources providing information about an upcoming resource or service sunset."];
  cites -> RFC8594;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC9745;
