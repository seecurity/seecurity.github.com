  RFC2533 [label="RFC2533
A Syntax for Describing Media Feature
Sets
1999", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document introduces and describes a syntax that can be used to define feature sets which are formed from combinations and relations involving individual media features. [STANDARDS-TRACK]", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2533;

