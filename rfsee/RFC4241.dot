digraph Flow {
  layout=twopi;
  root=RFC4241;
  overlap=false;
  RFC4241 [label="RFC4241
A Model of IPv6/IPv4 Dual Stack Internet
Access Service
2005", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo is a digest of the user network interface specification of NTT Communications' dual stack ADSL access service, which provide a IPv6/IPv4 dual stack services to home users.  In order to simplify user setup, these services have a mechanism to configure IPv6 specific parameters automatically.  The memo focuses on two basic parameters: the prefix assigned to the user and the addresses of IPv6 DNS servers, and it specifies a way to deliver these parameters to Customer Premises Equipment (CPE) automatically.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC4241;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC4241 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC4241 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC4241;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC4241 -> cites;
  RFC3177 [label="RFC3177
IAB/IESG Recommendations on IPv6 Address
Allocations to Sites
2001", shape=ellipse, URL="RFC3177.html", target="_top", tooltip="This document provides recommendations to the addressing registries (APNIC, ARIN and RIPE-NCC) on policies for assigning IPv6 address blocks to end sites.  In particular, it recommends the assignment of /48 in the general case, /64 when it is known that one and only one subnet is needed and /128 when it is absolutely known that one and only one device is connecting."];
  cites -> RFC3177;
  RFC3769 [label="RFC3769
Requirements for IPv6 Prefix Delegation
2004", shape=ellipse, URL="RFC3769.html", target="_top", tooltip="This document describes requirements for how IPv6 address prefixes should be delegated to an IPv6 subscriber's network (or site).  This memo provides information for the Internet community."];
  cites -> RFC3769;
  RFC1332 [label="RFC1332
The PPP Internet Protocol Control
Protocol (IPCP)
1992", shape=ellipse, URL="RFC1332.html", target="_top", tooltip="The Point-to-Point Protocol (PPP) [1] provides a standard method of encapsulating Network Layer protocol information over point-to-point links.  PPP also defines an extensible Link Control Protocol, and proposes a family of Network Control Protocols (NCPs) for establishing and configuring different network-layer protocols. [STANDARDS-TRACK]"];
  cites -> RFC1332;
  RFC2472 [label="RFC2472
IP Version 6 over PPP
1998", shape=ellipse, URL="RFC2472.html", target="_top", tooltip="This document defines the method for transmission of IP Version 6 packets over PPP links as well as the Network Control Protocol (NCP) for establishing and configuring the IPv6 over PPP.  It also specifies the method of forming IPv6 link-local addresses on PPP links. [STANDARDS-TRACK]"];
  cites -> RFC2472;
  RFC2516 [label="RFC2516
A Method for Transmitting PPP Over
Ethernet (PPPoE)
1999", shape=ellipse, URL="RFC2516.html", target="_top", tooltip="This document describes how to build PPP sessions and encapsulate PPP packets over Ethernet.  This memo provides information for the Internet community."];
  cites -> RFC2516;
  RFC2462 [label="RFC2462
IPv6 Stateless Address Autoconfiguration
1998", shape=ellipse, URL="RFC2462.html", target="_top", tooltip="This document specifies the steps a host takes in deciding how to autoconfigure its interfaces in IP version 6. [STANDARDS-TRACK]"];
  cites -> RFC2462;
  RFC3315 [label="RFC3315
Dynamic Host Configuration Protocol for
IPv6 (DHCPv6)
2003", shape=ellipse, URL="RFC3315.html", target="_top", tooltip="None"];
  cites -> RFC3315;
  RFC3633 [label="RFC3633
IPv6 Prefix Options for Dynamic Host
Configuration Protocol (DHCP) version 6
2003", shape=ellipse, URL="RFC3633.html", target="_top", tooltip="The Prefix Delegation options provide a mechanism for automated delegation of IPv6 prefixes using the Dynamic Host Configuration Protocol (DHCP).  This mechanism is intended for delegating a long-lived prefix from a delegating router to a requesting router, across an administrative boundary, where the delegating router does not require knowledge about the topology of the links in the network to which the prefixes will be assigned."];
  cites -> RFC3633;
  RFC2461 [label="RFC2461
Neighbor Discovery for IP Version 6
(IPv6)
1998", shape=ellipse, URL="RFC2461.html", target="_top", tooltip="This document specifies the Neighbor Discovery protocol for IP Version 6. [STANDARDS-TRACK]"];
  cites -> RFC2461;
  RFC3646 [label="RFC3646
DNS Configuration options for Dynamic
Host Configuration Protocol for IPv6
(DHCPv6)
2003", shape=ellipse, URL="RFC3646.html", target="_top", tooltip="This document describes Dynamic Host Configuration Protocol for IPv6 (DHCPv6) options for passing a list of available DNS recursive name servers and a domain search list to a client."];
  cites -> RFC3646;
  RFC3736 [label="RFC3736
Stateless Dynamic Host Configuration
Protocol (DHCP) Service for IPv6
2004", shape=ellipse, URL="RFC3736.html", target="_top", tooltip="Stateless Dynamic Host Configuration Protocol service for IPv6 (DHCPv6) is used by nodes to obtain configuration information, such as the addresses of DNS recursive name servers, that does not require the maintenance of any dynamic state for individual clients.  A node that uses stateless DHCP must have obtained its IPv6 addresses through some other mechanism, typically stateless address autoconfiguration.  This document explains which parts of RFC 3315 must be implemented in each of the different kinds of DHCP agents so that agent can support stateless DHCP. [STANDARDS-TRACK]"];
  cites -> RFC3736;
  RFC4075 [label="RFC4075
Simple Network Time Protocol (SNTP)
Configuration Option for DHCPv6
2005", shape=ellipse, URL="RFC4075.html", target="_top", tooltip="This document describes a new DHCPv6 option for passing a list of Simple Network Time Protocol (SNTP) server addresses to a client. [STANDARDS-TRACK]"];
  cites -> RFC4075;
  RFC3319 [label="RFC3319
Dynamic Host Configuration Protocol
(DHCPv6) Options for Session Initiation
Protocol (SIP) Servers
2003", shape=ellipse, URL="RFC3319.html", target="_top", tooltip="None"];
  cites -> RFC3319;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC4241;
}
