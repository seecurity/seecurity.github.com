digraph Flow {
  layout=twopi;
  root=RFC8855;
  overlap=false;
  RFC8855 [label="RFC8855
The Binary Floor Control Protocol (BFCP)
2021", shape=ellipse, fillcolor="#e8f0ff", tooltip="Floor control is a means to manage joint or exclusive access to shared resources in a (multiparty) conferencing environment. Thereby, floor control complements other functions -- such as conference and media session setup, conference policy manipulation, and media control -- that are realized by other protocols. This document specifies the Binary Floor Control Protocol (BFCP). BFCP is used between floor participants and floor control servers, and between floor chairs (i.e., moderators) and floor control servers. This document obsoletes RFC 4582.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8855;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8855 -> obs;
  RFC4582 [label="RFC4582
The Binary Floor Control Protocol (BFCP)
2006", shape=ellipse, URL="RFC4582.html", target="_top", tooltip="Floor control is a means to manage joint or exclusive access to shared resources in a (multiparty) conferencing environment. Thereby, floor control complements other functions -- such as conference and media session setup, conference policy manipulation, and media control -- that are realized by other protocols. This document specifies the Binary Floor Control Protocol (BFCP). BFCP is used between floor participants and floor control servers, and between floor chairs (i.e., moderators) and floor control servers. [STANDARDS-TRACK]"];
  obs -> RFC4582;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8855 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC8855;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC8855 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC8855;
  RFC8856 [label="RFC8856
Session Description Protocol (SDP)
Format for Binary Floor Control Protocol
(BFCP) Streams
2021", shape=ellipse, URL="RFC8856.html", target="_top", tooltip="This document defines the Session Description Protocol (SDP) offer/answer procedures for negotiating and establishing Binary Floor Control Protocol (BFCP) streams. This document obsoletes RFC 4583."];
  RFC8856 -> rev_cites;
  RFC8857 [label="RFC8857
The WebSocket Protocol as a Transport
for the Binary Floor Control Protocol
(BFCP)
2021", shape=ellipse, URL="RFC8857.html", target="_top", tooltip="The WebSocket protocol enables two-way real-time communication between clients and servers.  This document specifies the use of Binary Floor Control Protocol (BFCP) as a new WebSocket subprotocol enabling a reliable transport mechanism between BFCP entities in new scenarios."];
  RFC8857 -> rev_cites;
  RFC8864 [label="RFC8864
Negotiation Data Channels Using the
Session Description Protocol (SDP)
2021", shape=ellipse, URL="RFC8864.html", target="_top", tooltip="Data channel setup can be done using either the in-band Data Channel Establishment Protocol (DCEP) or some out-of-band non-DCEP protocol.  This document specifies how the SDP (Session Description Protocol) offer/answer exchange can be used to achieve an out-of-band non-DCEP negotiation for establishing a data channel."];
  RFC8864 -> rev_cites;
