digraph Flow {
  layout=twopi;
  root=RFC3760;
  overlap=false;
  RFC3760 [label="RFC3760
Securely Available Credentials (SACRED)
- Credential Server Framework
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="As the number, and more particularly the number of different types, of devices connecting to the Internet increases, credential mobility becomes an issue for IETF standardization.  This document responds to the requirements on protocols for secure exchange of credentials listed in RFC 3157, by presenting an abstract protocol framework.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3760;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3760 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3760 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3760;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3760 -> cites;
  RFC3157 [label="RFC3157
Securely Available Credentials -
Requirements
2001", shape=ellipse, URL="RFC3157.html", target="_top", tooltip="This document describes requirements to be placed on Securely Available Credentials (SACRED) protocols.  This memo provides information for the Internet community."];
  cites -> RFC3157;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2246 [label="RFC2246
The TLS Protocol Version 1.0
1999", shape=ellipse, URL="RFC2246.html", target="_top", tooltip="This document specifies Version 1.0 of the Transport Layer Security (TLS) protocol.  The TLS protocol provides communications privacy over the Internet.  The protocol allows client/server applications to communicate in a way that is designed to prevent eavesdropping, tampering, or message forgery."];
  cites -> RFC2246;
  RFC2945 [label="RFC2945
The SRP Authentication and Key Exchange
System
2000", shape=ellipse, URL="RFC2945.html", target="_top", tooltip="This document describes a cryptographically strong network authentication mechanism known as the Secure Remote Password (SRP) protocol. [STANDARDS-TRACK]"];
  cites -> RFC2945;
  RFC3080 [label="RFC3080
The Blocks Extensible Exchange Protocol
Core
2001", shape=ellipse, URL="RFC3080.html", target="_top", tooltip="This memo describes a generic application protocol kernel for connection-oriented, asynchronous interactions called the BEEP (Blocks Extensible Exchange Protocol) core. [STANDARDS-TRACK]"];
  cites -> RFC3080;
  RFC3081 [label="RFC3081
Mapping the BEEP Core onto TCP
2001", shape=ellipse, URL="RFC3081.html", target="_top", tooltip="This memo describes how a BEEP (Blocks Extensible Exchange Protocol) session is mapped onto a single TCP (Transmission Control Protocol) connection. [STANDARDS-TRACK]"];
  cites -> RFC3081;
  RFC1945 [label="RFC1945
Hypertext Transfer Protocol -- HTTP/1.0
1996", shape=ellipse, URL="RFC1945.html", target="_top", tooltip="The Hypertext Transfer Protocol (HTTP) is an application-level protocol with the lightness and speed necessary for distributed, collaborative, hypermedia information systems.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1945;
  RFC2616 [label="RFC2616
Hypertext Transfer Protocol -- HTTP/1.1
1999", shape=ellipse, URL="RFC2616.html", target="_top", tooltip="HTTP has been in use by the World-Wide Web global information initiative since 1990.  This specification defines the protocol referred to as HTTP/1.1, and is an update to RFC 2068. [STANDARDS-TRACK]"];
  cites -> RFC2616;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3760;
  RFC3767 [label="RFC3767
Securely Available Credentials Protocol
2004", shape=ellipse, URL="RFC3767.html", target="_top", tooltip="This document describes a protocol whereby a user can acquire cryptographic credentials (e.g., private keys, PKCS #15 structures) from a credential server, using a workstation that has locally trusted software installed, but with no user-specific configuration.  The protocol's payloads are described in XML.  This memo also specifies a Blocks Extensible Exchange Protocol (BEEP) profile of the protocol.  Security requirements are met by mandating support for TLS and/or DIGEST-MD5 (through BEEP). [STANDARDS-TRACK]"];
  RFC3767 -> rev_cites;
  RFC6072 [label="RFC6072
Certificate Management Service for the
Session Initiation Protocol (SIP)
2011", shape=ellipse, URL="RFC6072.html", target="_top", tooltip="This document defines a credential service that allows Session Initiation Protocol (SIP) User Agents (UAs) to use a SIP event package to discover the certificates of other users.  This mechanism allows User Agents that want to contact a given Address-of-Record (AOR) to retrieve that AOR's certificate by subscribing to the credential service, which returns an authenticated response containing that certificate.  The credential service also allows users to store and retrieve their own certificates and private keys. [STANDARDS-TRACK]"];
  RFC6072 -> rev_cites;
  RFC8826 [label="RFC8826
Security Considerations for WebRTC
2021", shape=ellipse, URL="RFC8826.html", target="_top", tooltip="WebRTC is a protocol suite for use with real-time applications that can be deployed in browsers -- real-time communication on the Web.  This document defines the WebRTC threat model and analyzes the security threats of WebRTC in that model."];
  RFC8826 -> rev_cites;
}
