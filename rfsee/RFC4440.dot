  RFC4440 [label="RFC4440
IAB Thoughts on the Role of the Internet
Research Task Force (IRTF)
2006", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document is an Internet Architecture Board (IAB) report on the role of the Internet Research Task Force (IRTF), both on its own and in relationship to the IETF.  This document evolved from a discussion within the IAB as part of a process of appointing a new chair of the IRTF.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC4440;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC4440 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC4440 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC4440;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC4440 -> cites;
  RFC3160 [label="RFC3160
The Tao of IETF - A Novice's Guide to
the Internet Engineering Task Force
2001", shape=ellipse, URL="RFC3160.html", target="_top", tooltip="This document describes the inner workings of IETF meetings and Working Groups, discusses organizations related to the IETF, and introduces the standards process.  This memo provides information for the Internet community."];
  cites -> RFC3160;
  RFC2014 [label="RFC2014
IRTF Research Group Guidelines and
Procedures
1996", shape=ellipse, URL="RFC2014.html", target="_top", tooltip="This document describes the guidelines and procedures for formation and operation of IRTF Research Groups.  It describes the relationship between IRTF participants, Research Groups, the Internet Research Steering Group (IRSG) and the Internet Architecture Board (IAB).  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2014;
  RFC2309 [label="RFC2309
Recommendations on Queue Management and
Congestion Avoidance in the Internet
1998", shape=ellipse, URL="RFC2309.html", target="_top", tooltip="This memo presents two recommendations to the Internet community concerning measures to improve and preserve Internet performance.  It presents a strong recommendation for testing, standardization, and widespread deployment of active queue management in routers, to improve the performance of today's Internet.  It also urges a concerted effort of research, measurement, and ultimate deployment of router mechanisms to protect the Internet from flows that are not sufficiently responsive to congestion notification.  This memo provides information for the Internet community.  It does not specify an Internet standard of any kind."];
  cites -> RFC2309;
  RFC3932 [label="RFC3932
The IESG and RFC Editor Documents:
Procedures
2004", shape=ellipse, URL="RFC3932.html", target="_top", tooltip="This document describes the IESG's procedures for handling documents submitted for RFC publication via the RFC Editor, subsequent to the changes proposed by the IESG at the Seoul IETF, March 2004. This document updates procedures described in RFC 2026 and RFC 3710. This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC3932;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC4440;
  RFC7418 [label="RFC7418
An IRTF Primer for IETF Participants
2014", shape=ellipse, URL="RFC7418.html", target="_top", tooltip="This document provides a high-level description of things for Internet Engineering Task Force (IETF) participants to consider when bringing proposals for new research groups (RGs) into the Internet Research Task Force (IRTF).  This document emphasizes differences in expectations between the two organizations."];
  RFC7418 -> rev_cites;
  RFC7827 [label="RFC7827
The Role of the IRTF Chair
2016", shape=ellipse, URL="RFC7827.html", target="_top", tooltip="This document briefly describes the role of the Chair of the Internet Research Task Force (IRTF), discusses its duties, and outlines the skill set a candidate for the role should ideally have."];
  RFC7827 -> rev_cites;
