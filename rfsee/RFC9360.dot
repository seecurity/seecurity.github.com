  RFC9360 [label="RFC9360
CBOR Object Signing and Encryption
(COSE): Header Parameters for Carrying
and Referencing X.509 Certificates
2023", shape=ellipse, fillcolor="#e8f0ff", tooltip="The CBOR Object Signing and Encryption (COSE) message structure uses references to keys in general.  For some algorithms, additional properties are defined that carry parameters relating to keys as needed.  The COSE Key structure is used for transporting keys outside of COSE messages.  This document extends the way that keys can be identified and transported by providing attributes that refer to or contain X.509 certificates."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC9360;

  obs [label="obsoletes", shape=box];
  RFC9360 -> obs;

  updates [label="updates", shape=box];
  RFC9360 -> updates;

  updated_by [label="updates", shape=box];
