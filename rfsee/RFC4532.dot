  RFC4532 [label="RFC4532
Lightweight Directory Access Protocol
(LDAP) Who am I? Operation
2006", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification provides a mechanism for Lightweight Directory Access Protocol (LDAP) clients to obtain the authorization identity the server has associated with the user or application entity.  This mechanism is specified as an LDAP extended operation called the LDAP Who am I? operation. [STANDARDS-TRACK]", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
