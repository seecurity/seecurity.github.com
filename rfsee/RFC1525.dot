digraph Flow {
  layout=twopi;
  root=RFC1525;
  overlap=false;
  RFC1525 [label="RFC1525
Definitions of Managed Objects for
Source Routing Bridges
1993", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP based internets.  In particular, it defines objects for managing source routing and source routing transparent bridges.  These bridges are also required to implement relevant groups in the Bridge MIB. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1525;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1525 -> obs;
  RFC1286 [label="RFC1286
Definitions of Managed Objects for
Bridges
1991", shape=ellipse, URL="RFC1286.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP based internets.  In particular it defines objects for managing bridges based on the IEEE 802.1d draft standard between Local Area Network (LAN) segments.  This memo is an extension to the SNMP MIB. [STANDARDS-TRACK]"];
  obs -> RFC1286;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC1525 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC1525;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC1525 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC1525;
  RFC4188 [label="RFC4188
Definitions of Managed Objects for
Bridges
2005", shape=ellipse, URL="RFC4188.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets. In particular, it defines objects for managing MAC bridges based on the IEEE 802.1D-1998 standard between Local Area Network (LAN) segments. Provisions are made for the support of transparent bridging. Provisions are also made so that these objects apply to bridges connected by subnetworks other than LAN segments. The MIB module presented in this memo is a translation of the BRIDGE-MIB defined in RFC 1493 to the SMIv2 syntax. This memo obsoletes RFC 1493. [STANDARDS-TRACK]"];
  RFC4188 -> rev_cites;
  RFC4663 [label="RFC4663
Transferring MIB Work from IETF Bridge
MIB WG to IEEE 802.1 WG
2006", shape=ellipse, URL="RFC4663.html", target="_top", tooltip="This document describes the plan to transition responsibility for bridging-related MIB modules from the IETF Bridge MIB Working Group to the IEEE 802.1 Working Group, which develops the bridging technology the MIB modules are designed to manage.  This memo provides information for the Internet community."];
  RFC4663 -> rev_cites;
  RFC4837 [label="RFC4837
Managed Objects of Ethernet Passive
Optical Networks (EPON)
2007", shape=ellipse, URL="RFC4837.html", target="_top", tooltip="This document defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP based Internets.  In particular, it defines objects for managing interfaces that conform to the Ethernet Passive Optical Networks (EPON) standard as defined in the IEEE Std 802.3ah-2004, which are extended capabilities to the Ethernet like interfaces. [STANDARDS-TRACK]"];
  RFC4837 -> rev_cites;
}
