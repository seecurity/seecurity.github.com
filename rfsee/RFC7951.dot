  RFC7951 [label="RFC7951
JSON Encoding of Data Modeled with YANG
2016", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines encoding rules for representing configuration data, state data, parameters of Remote Procedure Call (RPC) operations or actions, and notifications defined using YANG as JavaScript Object Notation (JSON) text.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC7951;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
