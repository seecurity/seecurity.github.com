digraph Flow {
  layout=twopi;
  root=RFC3737;
  overlap=false;
  RFC3737 [label="RFC3737
IANA Guidelines for the Registry of
Remote Monitoring (RMON) MIB modules
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the procedures for IANA to administer and maintain the Object Identifier (OID) tree under the Remote Monitoring (rmon) root.  This memo also documents the currently assigned values. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3737;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3737 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3737 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3737;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3737 -> cites;
  RFC2819 [label="RFC2819
Remote Network Monitoring Management
Information Base
2000", shape=ellipse, URL="RFC2819.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets.  In particular, it defines objects for managing remote network monitoring devices. [STANDARDS-TRACK]"];
  cites -> RFC2819;
  RFC1513 [label="RFC1513
Token Ring Extensions to the Remote
Network Monitoring MIB
1993", shape=ellipse, URL="RFC1513.html", target="_top", tooltip="This memo defines extensions to the Remote Network Monitoring MIB for managing 802.5 Token Ring networks. [STANDARDS-TRACK]"];
  cites -> RFC1513;
  RFC2021 [label="RFC2021
Remote Network Monitoring Management
Information Base Version 2 using SMIv2
1997", shape=ellipse, URL="RFC2021.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets.  In particular, it defines objects for managing remote network monitoring devices. [STANDARDS-TRACK]"];
  cites -> RFC2021;
  RFC3273 [label="RFC3273
Remote Network Monitoring Management
Information Base for High Capacity
Networks
2002", shape=ellipse, URL="RFC3273.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets.  In particular, it defines objects for managing remote network monitoring (RMON) devices for use on high speed networks.  This document contains a MIB Module that defines these new objects and also contains definitions of some updated objects from the RMON-MIB in RFC 2819 and the RMON2-MIB in RFC 2021. [PROPOSED STANDARD]"];
  cites -> RFC3273;
  RFC2613 [label="RFC2613
Remote Network Monitoring MIB Extensions
for Switched Networks Version 1.0
1999", shape=ellipse, URL="RFC2613.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets.  In particular, it defines objects for managing remote network monitoring devices in switched networks environments. [STANDARDS-TRACK]"];
  cites -> RFC2613;
  RFC3729 [label="RFC3729
Application Performance Measurement MIB
2004", shape=ellipse, URL="RFC3729.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in TCP/IP-based internets.  In particular, it defines objects for measuring the application performance as experienced by end-users. [STANDARDS-TRACK]"];
  cites -> RFC3729;
  RFC3287 [label="RFC3287
Remote Monitoring MIB Extensions for
Differentiated Services
2002", shape=ellipse, URL="RFC3287.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community.  In particular, it describes managed objects used for monitoring Differentiated Services (DS) Codepoint usage in packets which contain a DS field, utilizing the monitoring framework defined in the RMON-2 (Remote Network Monitoring Management Version 2) MIB. [STANDARDS-TRACK]"];
  cites -> RFC3287;
  RFC3144 [label="RFC3144
Remote Monitoring MIB Extensions for
Interface Parameters Monitoring
2001", shape=ellipse, URL="RFC3144.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community.  The document proposes an extension to the Remote Monitoring MIB with a method of sorting the interfaces of a monitored device according to values of parameters specific to this interface. [STANDARDS-TRACK]"];
  cites -> RFC3144;
  RFC3434 [label="RFC3434
Remote Monitoring MIB Extensions for
High Capacity Alarms
2002", shape=ellipse, URL="RFC3434.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community.  In particular, it describes managed objects for extending the alarm thresholding capabilities found in the Remote Monitoring (RMON) MIB (RFC 2819), to provide similar threshold monitoring of objects based on the Counter64 data type. [STANDARDS-TRACK]"];
  cites -> RFC3434;
  RFC2434 [label="RFC2434
Guidelines for Writing an IANA
Considerations Section in RFCs
1998", shape=ellipse, URL="RFC2434.html", target="_top", tooltip="This document discusses issues that should be considered in formulating a policy for assigning values to a name space and provides guidelines to document authors on the specific text that must be included in documents that place demands on the IANA.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2434;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3737;
  RFC4711 [label="RFC4711
Real-time Application Quality-of-Service
Monitoring (RAQMON) MIB
2006", shape=ellipse, URL="RFC4711.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community.  The document proposes an extension to the Remote Monitoring MIB, RFC 2819.  In particular, it describes managed objects used for real-time application Quality of Service (QoS) monitoring. [STANDARDS-TRACK]"];
  RFC4711 -> rev_cites;
  RFC4712 [label="RFC4712
Transport Mappings for Real-time
Application Quality-of-Service
Monitoring (RAQMON) Protocol Data Unit
(PDU)
2006", shape=ellipse, URL="RFC4712.html", target="_top", tooltip="This memo specifies two transport mappings of the \%Real-Time Application Quality-of-Service Monitoring (RAQMON) information model defined in RFC 4710 using TCP as a native transport and the Simple Network Management Protocol (SNMP) to carry the RAQMON information from a RAQMON Data Source (RDS) to a RAQMON Report Collector (RRC). [STANDARDS-TRACK]"];
  RFC4712 -> rev_cites;
}
