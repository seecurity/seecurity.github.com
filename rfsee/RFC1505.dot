  RFC1505 [label="RFC1505
Encoding Header Field for Internet
Messages
1993", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document expands upon the elective experimental Encoding header field which permits the mailing of multi-part, multi-structured messages.  It replaces RFC 1154.  This memo defines an Experimental Protocol for the Internet community.  It does not specify an Internet standard.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1505;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1505 -> obs;
  RFC1154 [label="RFC1154
Encoding header field for internet
messages
1990", shape=ellipse, URL="RFC1154.html", target="_top", tooltip="This RFC proposes an elective experimental Encoding header field to permit the mailing of multi-part, multi-structured messages.  The use of Encoding updates RFC 1049 (Content-Type), and is a suggested update to RFCs 1113, 1114, and 1115 (Privacy Enhancement)."];
  obs -> RFC1154;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC1505 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC1505;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC1505 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC1505;
  RFC1700 [label="RFC1700
Assigned Numbers
1994", shape=ellipse, URL="RFC1700.html", target="_top", tooltip="This RFC is a snapshot of the ongoing process of the assignment of protocol parameters for the Internet protocol suite.  To make the current information readily available the assignments are kept up-to- date in a set of online text files.  This memo is a status report on the parameters (i.e., numbers and keywords) used in protocols in the Internet community."];
  RFC1700 -> rev_cites;
