  RFC3758 [label="RFC3758
Stream Control Transmission Protocol
(SCTP) Partial Reliability Extension
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo describes an extension to the Stream Control Transmission Protocol (SCTP) that allows an SCTP endpoint to signal to its peer that it should move the cumulative ack point forward.  When both sides of an SCTP association support this extension, it can be used by an SCTP implementation to provide partially reliable data transmission service to an upper layer protocol.  This memo describes the protocol extensions, which consist of a new parameter for INIT and INIT ACK, and a new FORWARD TSN chunk type, and provides one example of a partially reliable service that can be provided to the upper layer via this mechanism. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3758;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3758 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3758 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3758;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3758 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
