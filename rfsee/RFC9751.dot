  RFC9751 [label="RFC9751
Closing the RTP Payload Format Media
Types Registry
2025", shape=ellipse, fillcolor="#e8f0ff", tooltip="The working group process and the authors of RTP payload formats have sometimes failed to ensure that the media types are registered in the IANA RTP Payload Format Media Types registry as recommended by RFC 8088.  To simplify the process and rely only on the Media Types registry, this document closes the RTP payload- specific registry.  In addition, it updates the instruction in RFC 8088 to reflect this change.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC9751;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC9751 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC9751 -> updates;
  RFC8088 [label="RFC8088
How to Write an RTP Payload Format
2017", shape=ellipse, URL="RFC8088.html", target="_top", tooltip="This document contains information on how best to write an RTP payload format specification.  It provides reading tips, design practices, and practical tips on how to produce an RTP payload format specification quickly and with good results.  A template is also included with instructions."];
  updates -> RFC8088;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC9751;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC9751 -> cites;
  RFC8088 [label="RFC8088
How to Write an RTP Payload Format
2017", shape=ellipse, URL="RFC8088.html", target="_top", tooltip="This document contains information on how best to write an RTP payload format specification.  It provides reading tips, design practices, and practical tips on how to produce an RTP payload format specification quickly and with good results.  A template is also included with instructions."];
  cites -> RFC8088;
  RFC4855 [label="RFC4855
Media Type Registration of RTP Payload
Formats
2007", shape=ellipse, URL="RFC4855.html", target="_top", tooltip="This document specifies the procedure to register RTP payload formats as audio, video, or other media subtype names.  This is useful in a text-based format description or control protocol to identify the type of an RTP transmission. [STANDARDS-TRACK]"];
  cites -> RFC4855;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC9751;
