digraph Flow {
  layout=twopi;
  root=RFC9727;
  overlap=false;
  RFC9727 [label="RFC9727
api-catalog: A Well-Known URI and Link
Relation to Help Discovery of APIs
2025", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the api-catalog well-known URI and link relation.  It is intended to facilitate automated discovery and usage of published Application Programming Interfaces (APIs).  A request to the api-catalog resource will return a document providing information about, and links to, the Publisher's APIs.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC9727;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC9727 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC9727 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC9727;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC9727 -> cites;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC8174 [label="RFC8174
Ambiguity of Uppercase vs Lowercase in
RFC 2119 Key Words
2017", shape=ellipse, URL="RFC8174.html", target="_top", tooltip="RFC 2119 specifies common key words that may be used in protocol specifications.  This document aims to reduce the ambiguity by clarifying that only UPPERCASE usage of the key words have the defined special meanings."];
  cites -> RFC8174;
  RFC6573 [label="RFC6573
The Item and Collection Link Relations
2012", shape=ellipse, URL="RFC6573.html", target="_top", tooltip="RFC 5988 standardized a means of indicating the relationships between resources on the Web.  This specification defines a pair of reciprocal link relation types that may be used to express the relationship between a collection and its members.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  cites -> RFC6573;
  RFC9264 [label="RFC9264
Linkset: Media Types and a Link Relation
Type for Link Sets
2022", shape=ellipse, URL="RFC9264.html", target="_top", tooltip="This specification defines two formats and associated media types for representing sets of links as standalone documents.  One format is based on JSON, and the other is aligned with the format for representing links in the HTTP Link header field.  This specification also introduces a link relation type to support the discovery of sets of links."];
  cites -> RFC9264;
  RFC7284 [label="RFC7284
The Profile URI Registry
2014", shape=ellipse, URL="RFC7284.html", target="_top", tooltip="This document defines a registry for profile URIs to be used in specifications standardizing profiles."];
  cites -> RFC7284;
  RFC8631 [label="RFC8631
Link Relation Types for Web Services
2019", shape=ellipse, URL="RFC8631.html", target="_top", tooltip="Many resources provided on the Web are part of sets of resources that are provided in a context that is managed by one particular service provider.  Often, these sets of resources are referred to as Web services or Web APIs.  This specification defines link relations that represent relationships from Web services or APIs to resources that provide documentation, descriptions, metadata, or status information for these resources.  Documentation is primarily intended for human consumers, whereas descriptions are primarily intended for automated consumers.  Metadata provides information about a service's context.  This specification also defines a link relation to identify status resources that are used to represent information about service status."];
  cites -> RFC8631;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC9727;
}
