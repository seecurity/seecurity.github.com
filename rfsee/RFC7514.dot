  RFC7514 [label="RFC7514
Really Explicit Congestion Notification
(RECN)
2015", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document proposes a new ICMP message that a router or host may use to advise a host to reduce the rate at which it sends, in cases where the host ignores other signals provided by packet loss and Explicit Congestion Notification (ECN).", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC7514;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC7514 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC7514 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC7514;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC7514 -> cites;
  RFC3168 [label="RFC3168
The Addition of Explicit Congestion
Notification (ECN) to IP
2001", shape=ellipse, URL="RFC3168.html", target="_top", tooltip="This memo specifies the incorporation of ECN (Explicit Congestion Notification) to TCP and IP, including ECN's use of two bits in the IP header. [STANDARDS-TRACK]"];
  cites -> RFC3168;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2460 [label="RFC2460
Internet Protocol, Version 6 (IPv6)
Specification
1998", shape=ellipse, URL="RFC2460.html", target="_top", tooltip="This document specifies version 6 of the Internet Protocol (IPv6), also sometimes referred to as IP Next Generation or IPng. [STANDARDS-TRACK]"];
  cites -> RFC2460;
  RFC6633 [label="RFC6633
Deprecation of ICMP Source Quench
Messages
2012", shape=ellipse, URL="RFC6633.html", target="_top", tooltip="This document formally deprecates the use of ICMP Source Quench messages by transport protocols, formally updating RFC 792, RFC 1122, and RFC 1812. [STANDARDS-TRACK]"];
  cites -> RFC6633;
  RFC5927 [label="RFC5927
ICMP Attacks against TCP
2010", shape=ellipse, URL="RFC5927.html", target="_top", tooltip="This document discusses the use of the Internet Control Message Protocol (ICMP) to perform a variety of attacks against the Transmission Control Protocol (TCP).  Additionally, this document describes a number of widely implemented modifications to TCP's handling of ICMP error messages that help to mitigate these issues.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  cites -> RFC5927;
  RFC3514 [label="RFC3514
The Security Flag in the IPv4 Header
2003", shape=ellipse, URL="RFC3514.html", target="_top", tooltip="Firewalls, packet filters, intrusion detection systems, and the like often have difficulty distinguishing between packets that have malicious intent and those that are merely unusual.  We define a security flag in the IPv4 header as a means of distinguishing the two cases.  This memo provides information for the Internet community."];
  cites -> RFC3514;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC7514;
