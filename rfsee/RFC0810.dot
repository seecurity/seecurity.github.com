  RFC0810 [label="RFC0810
DoD Internet host table specification
1982", shape=ellipse, fillcolor="#e8f0ff", tooltip="This RFC specifies a new host table format applicable to both ARPANET and Internet needs.  In addition to host name to host address translation and selected protocol information, we have also included network and gateway name to address correspondence, and host operating system information.  This RFC obsoletes the host table described in RFC 608."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC0810;
  RFC0952 [label="RFC0952
DoD Internet host table specification
1985", shape=ellipse, URL="RFC0952.html", target="_top", tooltip="This RFC is the official specification of the format of the Internet Host Table.  This edition of the specification includes minor revisions to RFC-810 which brings it up to date."];
  RFC0952 -> obs_by;

  obs [label="obsoletes", shape=box];
  RFC0810 -> obs;
  RFC0608 [label="RFC0608
Host names on-line
1974", shape=ellipse, URL="RFC0608.html", target="_top", tooltip="Response to RFC 606; see also RFCs 627, 625 and 623."];
  obs -> RFC0608;

  updates [label="updates", shape=box];
  RFC0810 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC0810;

  cites [label="cites", shape=box];
