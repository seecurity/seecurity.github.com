  RFC4639 [label="RFC4639
Cable Device Management Information Base
for Data-Over-Cable Service Interface
Specification (DOCSIS) Compliant Cable
Modems and Cable Modem Termination
Systems
2006", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community. In particular, it defines a basic set of managed objects for Simple Network Management Protocol (SNMP)-based management of Data Over Cable Service Interface Specification (DOCSIS)-compliant Cable Modems and Cable Modem Termination Systems. This memo obsoletes RFC 2669. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC4639;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC4639 -> obs;
  RFC2669 [label="RFC2669
DOCSIS Cable Device MIB Cable Device
Management Information Base for DOCSIS
compliant Cable Modems and Cable Modem
Termination Systems
1999", shape=ellipse, URL="RFC2669.html", target="_top", tooltip="This memo defines a portion of the Management Information Base (MIB) for use with network management protocols in the Internet community.  In particular, it defines a basic set of managed objects for SNMP-based management of DOCSIS 1.0 compliant Cable Modems and Cable Modem Termination Systems. [STANDARDS-TRACK]"];
  obs -> RFC2669;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
