  RFC3928 [label="RFC3928
Lightweight Directory Access Protocol
(LDAP) Client Update Protocol (LCUP)
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the Lightweight Directory Access Protocol (LDAP) Client Update Protocol (LCUP).  The protocol is intended to allow an LDAP client to synchronize with the content of a directory information tree (DIT) stored by an LDAP server and to be notified about the changes to that content. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3928;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3928 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3928 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3928;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3928 -> cites;
  RFC3384 [label="RFC3384
Lightweight Directory Access Protocol
(version 3) Replication Requirements
2002", shape=ellipse, URL="RFC3384.html", target="_top", tooltip="None"];
  cites -> RFC3384;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2251 [label="RFC2251
Lightweight Directory Access Protocol
(v3)
1997", shape=ellipse, URL="RFC2251.html", target="_top", tooltip="The protocol described in this document is designed to provide access to directories supporting the X.500 models, while not incurring the resource requirements of the X.500 Directory Access Protocol (DAP). [STANDARDS-TRACK]"];
  cites -> RFC2251;
  RFC3383 [label="RFC3383
Internet Assigned Numbers Authority
(IANA) Considerations for the
Lightweight Directory Access Protocol
(LDAP)
2002", shape=ellipse, URL="RFC3383.html", target="_top", tooltip="None"];
  cites -> RFC3383;
  RFC3672 [label="RFC3672
Subentries in the Lightweight Directory
Access Protocol (LDAP)
2003", shape=ellipse, URL="RFC3672.html", target="_top", tooltip="In X.500 directories, subentries are special entries used to hold information associated with a subtree or subtree refinement.  This document adapts X.500 subentries mechanisms for use with the Lightweight Directory Access Protocol (LDAP)."];
  cites -> RFC3672;
  RFC3671 [label="RFC3671
Collective Attributes in the Lightweight
Directory Access Protocol (LDAP)
2003", shape=ellipse, URL="RFC3671.html", target="_top", tooltip="X.500 collective attributes allow common characteristics to be shared between collections of entries.  This document summarizes the X.500 information model for collective attributes and describes use of collective attributes in LDAP (Lightweight Directory Access Protocol).  This document provides schema definitions for collective attributes for use in LDAP."];
  cites -> RFC3671;
  RFC3909 [label="RFC3909
Lightweight Directory Access Protocol
(LDAP) Cancel Operation
2004", shape=ellipse, URL="RFC3909.html", target="_top", tooltip="This specification describes a Lightweight Directory Access Protocol (LDAP) extended operation to cancel (or abandon) an outstanding operation.  Unlike the LDAP Abandon operation, but like the X.511 Directory Access Protocol (DAP) Abandon operation, this operation has a response which provides an indication of its outcome. [STANDARDS-TRACK]"];
  cites -> RFC3909;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3928;
  RFC4533 [label="RFC4533
The Lightweight Directory Access
Protocol (LDAP) Content Synchronization
Operation
2006", shape=ellipse, URL="RFC4533.html", target="_top", tooltip="This specification describes the Lightweight Directory Access Protocol (LDAP) Content Synchronization Operation.  The operation allows a client to maintain a copy of a fragment of the Directory Information Tree (DIT).  It supports both polling for changes and listening for changes.  The operation is defined as an extension of the LDAP Search Operation.  This memo defines an Experimental Protocol for the Internet community."];
  RFC4533 -> rev_cites;
