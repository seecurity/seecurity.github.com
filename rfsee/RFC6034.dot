digraph Flow {
  layout=twopi;
  root=RFC6034;
  overlap=false;
  RFC6034 [label="RFC6034
Unicast-Prefix-Based IPv4 Multicast
Addresses
2010", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification defines an extension to the multicast addressing architecture of the IP Version 4 protocol.  The extension presented in this document allows for unicast-prefix-based assignment of multicast addresses.  By delegating multicast addresses at the same time as unicast prefixes, network operators will be able to identify their multicast addresses without needing to run an inter-domain allocation protocol. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC6034;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC6034 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC6034 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC6034;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC6034 -> cites;
  RFC3180 [label="RFC3180
GLOP Addressing in 233/8
2001", shape=ellipse, URL="RFC3180.html", target="_top", tooltip="This document defines the policy for the use of 233/8 for statically e assigned multicast addresses.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC3180;
  RFC4893 [label="RFC4893
BGP Support for Four-octet AS Number
Space
2007", shape=ellipse, URL="RFC4893.html", target="_top", tooltip="Currently the Autonomous System (AS) number is encoded as a two-octet entity in BGP.  This document describes extensions to BGP to carry the Autonomous System number as a four-octet entity. [STANDARDS-TRACK]"];
  cites -> RFC4893;
  RFC2730 [label="RFC2730
Multicast Address Dynamic Client
Allocation Protocol (MADCAP)
1999", shape=ellipse, URL="RFC2730.html", target="_top", tooltip="This document defines a protocol, Multicast Address Dynamic Client Allocation Protocol (MADCAP), that allows hosts to request multicast addresses from multicast address allocation servers. [STANDARDS-TRACK]"];
  cites -> RFC2730;
  RFC3306 [label="RFC3306
Unicast-Prefix-based IPv6 Multicast
Addresses
2002", shape=ellipse, URL="RFC3306.html", target="_top", tooltip="None"];
  cites -> RFC3306;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC6034;
  RFC6308 [label="RFC6308
Overview of the Internet Multicast
Addressing Architecture
2011", shape=ellipse, URL="RFC6308.html", target="_top", tooltip="The lack of up-to-date documentation on IP multicast address allocation and assignment procedures has caused a great deal of confusion.  To clarify the situation, this memo describes the allocation and assignment techniques and mechanisms currently (as of this writing) in use.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  RFC6308 -> rev_cites;
  RFC6676 [label="RFC6676
Multicast Addresses for Documentation
2012", shape=ellipse, URL="RFC6676.html", target="_top", tooltip="This document discusses which multicast addresses should be used for documentation purposes and reserves multicast addresses for such use.  Some multicast addresses are derived from AS numbers or unicast addresses.  This document also explains how these can be used for documentation purposes.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  RFC6676 -> rev_cites;
  RFC7042 [label="RFC7042
IANA Considerations and IETF Protocol
and Documentation Usage for IEEE 802
Parameters
2013", shape=ellipse, URL="RFC7042.html", target="_top", tooltip="Some IETF protocols make use of Ethernet frame formats and IEEE 802 parameters.  This document discusses several uses of such parameters in IETF protocols, specifies IANA considerations for assignment of points under the IANA OUI (Organizationally Unique Identifier), and provides some values for use in documentation.  This document obsoletes RFC 5342."];
  RFC7042 -> rev_cites;
  RFC9542 [label="RFC9542
IANA Considerations and IETF Protocol
and Documentation Usage for IEEE 802
Parameters
2024", shape=ellipse, URL="RFC9542.html", target="_top", tooltip="Some IETF protocols make use of Ethernet frame formats and IEEE 802 parameters.  This document discusses several aspects of such parameters and their use in IETF protocols, specifies IANA considerations for assignment of points under the IANA Organizationally Unique Identifier (OUI), and provides some values for use in documentation.  This document obsoletes RFC 7042."];
  RFC9542 -> rev_cites;
}
