  RFC7396 [label="RFC7396
JSON Merge Patch
2014", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification defines the JSON merge patch format and processing rules.  The merge patch format is primarily intended for use with the HTTP PATCH method as a means of describing a set of modifications to a target resource's content.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

