  RFC6716 [label="RFC6716
Definition of the Opus Audio Codec
2012", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the Opus interactive speech and audio codec.  Opus is designed to handle a wide range of interactive audio applications, including Voice over IP, videoconferencing, in-game chat, and even live, distributed music performances.  It scales from low bitrate narrowband speech at 6 kbit/s to very high quality stereo music at 510 kbit/s.  Opus uses both Linear Prediction (LP) and the Modified Discrete Cosine Transform (MDCT) to achieve good compression of both speech and music. [STANDARDS-TRACK]", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC6716;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC6716 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC6716 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
