  RFC8251 -> rev_cites;
  RFC8486 [label="RFC8486
Ambisonics in an Ogg Opus Container
2018", shape=ellipse, URL="RFC8486.html", target="_top", tooltip="This document defines an extension to the Opus audio codec to encapsulate coded Ambisonics using the Ogg format.  It also contains updates to RFC 7845 to reflect necessary changes in the description of channel mapping families."];
  RFC8486 -> rev_cites;
