digraph Flow {
  layout=twopi;
  root=RFC3936;
  overlap=false;
  RFC3936 [label="RFC3936
Procedures for Modifying the Resource
reSerVation Protocol (RSVP)
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo specifies procedures for modifying the Resource reSerVation Protocol (RSVP).  This memo also lays out new assignment guidelines for number spaces for RSVP messages, object classes, class-types, and sub-objects.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3936;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3936 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3936 -> updates;
  RFC3209 [label="RFC3209
RSVP-TE: Extensions to RSVP for LSP
Tunnels
2001", shape=ellipse, URL="RFC3209.html", target="_top", tooltip="This document describes the use of RSVP (Resource Reservation Protocol), including all the necessary extensions, to establish label-switched paths (LSPs) in MPLS (Multi-Protocol Label Switching).  Since the flow along an LSP is completely identified by the label applied at the ingress node of the path, these paths may be treated as tunnels.  A key application of LSP tunnels is traffic engineering with MPLS as specified in RFC 2702. [STANDARDS-TRACK]"];
  updates -> RFC3209;
  RFC2205 [label="RFC2205
Resource ReSerVation Protocol (RSVP) --
Version 1 Functional Specification
1997", shape=ellipse, URL="RFC2205.html", target="_top", tooltip="This memo describes version 1 of RSVP, a resource reservation setup protocol designed for an integrated services Internet.  RSVP provides receiver-initiated setup of resource reservations for multicast or unicast data flows, with good scaling and robustness properties. [STANDARDS-TRACK]"];
  updates -> RFC2205;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3936;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3936 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3936;
  RFC5978 [label="RFC5978
Using and Extending the NSIS Protocol
Family
2010", shape=ellipse, URL="RFC5978.html", target="_top", tooltip="This document gives an overview of the Next Steps in Signaling (NSIS) framework and protocol suite created by the NSIS Working Group during the period of 2001-2010.  It also includes suggestions on how the industry can make use of the new protocols and how the community can exploit the extensibility of both the framework and existing protocols to address future signaling needs.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  RFC5978 -> rev_cites;
  RFC8424 [label="RFC8424
Extensions to RSVP-TE for Label Switched
Path (LSP) Ingress Fast Reroute (FRR)
Protection
2018", shape=ellipse, URL="RFC8424.html", target="_top", tooltip="This document describes extensions to Resource Reservation Protocol - Traffic Engineering (RSVP-TE) for locally protecting the ingress node of a Point-to-Point (P2P) or Point-to-Multipoint (P2MP) Traffic Engineered (TE) Label Switched Path (LSP).  It extends the Fast Reroute (FRR) protection for transit nodes of an LSP to the ingress node of the LSP.  The procedures described in this document are experimental."];
  RFC8424 -> rev_cites;
}
