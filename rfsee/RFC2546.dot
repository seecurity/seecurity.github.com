digraph Flow {
  layout=twopi;
  root=RFC2546;
  overlap=false;
  RFC2546 [label="RFC2546
6Bone Routing Practice
1999", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo identifies guidelines on how 6Bone sites might operate, so that the 6Bone can remain a quality experimentation environment and to avoid pathological situations that have been encountered in the past.  It defines the 'best current practice' acceptable in the 6Bone for the configuration of both Interior Gateway Protocols and Exterior Gateway Protocols.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2546;
  RFC2772 [label="RFC2772
6Bone Backbone Routing Guidelines
2000", shape=ellipse, URL="RFC2772.html", target="_top", tooltip="This document provides a set of guidelines for all 6bone routing equipment operators to use as a reference for efficient and stable deployment of 6bone routing systems.  This memo provides information for the Internet community."];
  RFC2772 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2546 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2546 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2546;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2546 -> cites;
  RFC2080 [label="RFC2080
RIPng for IPv6
1997", shape=ellipse, URL="RFC2080.html", target="_top", tooltip="This document specifies a routing protocol for an IPv6 internet.  It is based on protocols and algorithms currently in wide use in the IPv4 Internet [STANDARDS-TRACK]"];
  cites -> RFC2080;
  RFC2283 [label="RFC2283
Multiprotocol Extensions for BGP-4
1998", shape=ellipse, URL="RFC2283.html", target="_top", tooltip="This document defines extensions to BGP-4 to enable it to carry routing information for multiple Network Layer protocols (e.g., IPv6, IPX, etc...).  The extensions are backward compatible - a router that supports the extensions can interoperate with a router that doesn't support the extensions. [STANDARDS-TRACK]"];
  cites -> RFC2283;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2471 [label="RFC2471
IPv6 Testing Address Allocation
1998", shape=ellipse, URL="RFC2471.html", target="_top", tooltip="This document describes an allocation plan for IPv6 addresses to be used in testing IPv6 prototype software.  This memo defines an Experimental Protocol for the Internet community."];
  cites -> RFC2471;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2546;
  RFC2772 [label="RFC2772
6Bone Backbone Routing Guidelines
2000", shape=ellipse, URL="RFC2772.html", target="_top", tooltip="This document provides a set of guidelines for all 6bone routing equipment operators to use as a reference for efficient and stable deployment of 6bone routing systems.  This memo provides information for the Internet community."];
  RFC2772 -> rev_cites;
}
