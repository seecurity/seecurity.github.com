digraph Flow {
  layout=twopi;
  root=RFC5901;
  overlap=false;
  RFC5901 [label="RFC5901
Extensions to the IODEF-Document Class
for Reporting Phishing
2010", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document extends the Incident Object Description Exchange Format (IODEF) defined in RFC 5070 to support the reporting of phishing events, which is a particular type of fraud.  These extensions are flexible enough to support information gleaned from activities throughout the entire electronic fraud cycle -- from receipt of the phishing lure to the disablement of the collection site.  Both simple reporting and complete forensic reporting are possible, as is consolidating multiple incidents. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC5901;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC5901 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC5901 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC5901;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC5901 -> cites;
  RFC5070 [label="RFC5070
The Incident Object Description Exchange
Format
2007", shape=ellipse, URL="RFC5070.html", target="_top", tooltip="The Incident Object Description Exchange Format (IODEF) defines a data representation that provides a framework for sharing information commonly exchanged by Computer Security Incident Response Teams (CSIRTs) about computer security incidents.  This document describes the information model for the IODEF and provides an associated data model specified with XML Schema. [STANDARDS-TRACK]"];
  cites -> RFC5070;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC3982 [label="RFC3982
IRIS:  A Domain Registry (dreg) Type for
the Internet Registry Information
Service (IRIS)
2005", shape=ellipse, URL="RFC3982.html", target="_top", tooltip="This document describes an Internet Registry Information Service (IRIS) registry schema for registered DNS information.  The schema extends the necessary query and result operations of IRIS to provide the functional information service needs for syntaxes and results used by domain registries and registrars. [STANDARDS-TRACK]"];
  cites -> RFC3982;
  RFC3275 [label="RFC3275
(Extensible Markup Language) XML-
Signature Syntax and Processing
2002", shape=ellipse, URL="RFC3275.html", target="_top", tooltip="This document specifies XML (Extensible Markup Language) digital signature processing rules and syntax. [STANDARDS-TRACK]"];
  cites -> RFC3275;
  RFC5322 [label="RFC5322
Internet Message Format
2008", shape=ellipse, URL="RFC5322.html", target="_top", tooltip="This document specifies the Internet Message Format (IMF), a syntax for text messages that are sent between computer users, within the framework of electronic mail messages.  This specification is a revision of Request For Comments (RFC) 2822, which itself superseded Request For Comments (RFC) 822, Standard for the Format of ARPA Internet Text Messages, updating it to reflect current practice and incorporating incremental changes that were specified in other RFCs. [STANDARDS-TRACK]"];
  cites -> RFC5322;
  RFC3688 [label="RFC3688
The IETF XML Registry
2004", shape=ellipse, URL="RFC3688.html", target="_top", tooltip="This document describes an IANA maintained registry for IETF standards which use Extensible Markup Language (XML) related items such as Namespaces, Document Type Declarations (DTDs), Schemas, and Resource Description Framework (RDF) Schemas."];
  cites -> RFC3688;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC5901;
  RFC7970 [label="RFC7970
The Incident Object Description Exchange
Format Version 2
2016", shape=ellipse, URL="RFC7970.html", target="_top", tooltip="The Incident Object Description Exchange Format (IODEF) defines a data representation for security incident reports and indicators commonly exchanged by operational security teams for mitigation and watch and warning.  This document describes an updated information model for the IODEF and provides an associated data model specified with the XML schema.  This new information and data model obsoletes RFCs 5070 and 6685."];
  RFC7970 -> rev_cites;
  RFC8134 [label="RFC8134
Management Incident Lightweight Exchange
(MILE) Implementation Report
2017", shape=ellipse, URL="RFC8134.html", target="_top", tooltip="This document is a collection of implementation reports from vendors, consortiums, and researchers who have implemented one or more of the standards published from the IETF INCident Handling (INCH) and Management Incident Lightweight Exchange (MILE) working groups."];
  RFC8134 -> rev_cites;
  RFC8274 [label="RFC8274
Incident Object Description Exchange
Format Usage Guidance
2017", shape=ellipse, URL="RFC8274.html", target="_top", tooltip="The Incident Object Description Exchange Format (IODEF) v2 (RFC7970) defines a data representation that provides a framework for sharing information about computer security incidents commonly exchanged by Computer Security Incident Response Teams (CSIRTs) .  Since the IODEF model includes a wealth of available options that can be used to describe a security incident or issue, it can be challenging for security practitioners to develop tools that leverage IODEF for incident sharing.  This document provides guidelines for IODEF implementers.  It addresses how common security indicators can be represented in IODEF and use-cases of how IODEF is being used.  This document aims to make IODEF's adoption by vendors easier and encourage faster and wider adoption of the model by CSIRTs around the world."];
  RFC8274 -> rev_cites;
}
