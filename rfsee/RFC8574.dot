digraph Flow {
  layout=twopi;
  root=RFC8574;
  overlap=false;
  RFC8574 [label="RFC8574
cite-as: A Link Relation to Convey a
Preferred URI for Referencing
2019", shape=ellipse, fillcolor="#e8f0ff", tooltip="A web resource is routinely referenced by means of the URI with which it is directly accessed.  But cases exist where referencing a resource by means of a different URI is preferred.  This specification defines a link relation type that can be used to convey such a preference.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8574;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8574 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8574 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC8574;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC8574 -> cites;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC8174 [label="RFC8174
Ambiguity of Uppercase vs Lowercase in
RFC 2119 Key Words
2017", shape=ellipse, URL="RFC8174.html", target="_top", tooltip="RFC 2119 specifies common key words that may be used in protocol specifications.  This document aims to reduce the ambiguity by clarifying that only UPPERCASE usage of the key words have the defined special meanings."];
  cites -> RFC8174;
  RFC8288 [label="RFC8288
Web Linking
2017", shape=ellipse, URL="RFC8288.html", target="_top", tooltip="This specification defines a model for the relationships between resources on the Web (links) and the type of those relationships (link relation types). It also defines the serialisation of such links in HTTP headers with the Link header field."];
  cites -> RFC8288;
  RFC5988 [label="RFC5988
Web Linking
2010", shape=ellipse, URL="RFC5988.html", target="_top", tooltip="This document specifies relation types for Web links, and defines a registry for them.  It also defines the use of such links in HTTP headers with the Link header field. [STANDARDS-TRACK]"];
  cites -> RFC5988;
  RFC4287 [label="RFC4287
The Atom Syndication Format
2005", shape=ellipse, URL="RFC4287.html", target="_top", tooltip="This document specifies Atom, an XML-based Web content and metadata syndication format. [STANDARDS-TRACK]"];
  cites -> RFC4287;
  RFC6249 [label="RFC6249
Metalink/HTTP: Mirrors and Hashes
2011", shape=ellipse, URL="RFC6249.html", target="_top", tooltip="This document specifies Metalink/HTTP: Mirrors and Cryptographic Hashes in HTTP header fields, a different way to get information that is usually contained in the Metalink XML-based download description format.  Metalink/HTTP describes multiple download locations (mirrors), Peer-to-Peer, cryptographic hashes, digital signatures, and other information using existing standards for HTTP header fields.  Metalink clients can use this information to make file transfers more robust and reliable.  Normative requirements for Metalink/HTTP clients and servers are described here. [STANDARDS-TRACK]"];
  cites -> RFC6249;
  RFC6596 [label="RFC6596
The Canonical Link Relation
2012", shape=ellipse, URL="RFC6596.html", target="_top", tooltip="RFC 5988 specifies a way to define relationships between links on the web.  This document describes a new type of such a relationship, canonical, to designate an Internationalized Resource Identifier (IRI) as preferred over resources with duplicative content.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  cites -> RFC6596;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC8574;
}
