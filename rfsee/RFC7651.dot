digraph Flow {
  rankdir=LR;
  splines=true;
  concentrate=true;   // merge parallel edges / reduce clutter
  nodesep=0.25;       // horizontal/vertical separation between nodes
  ranksep=0.35;       // separation between ranks (columns in LR)
  RFC7651 [label="RFC7651
3GPP IP Multimedia Subsystems (IMS)
Option for the Internet Key Exchange
Protocol Version 2 (IKEv2)
2015", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines two new configuration attributes for the Internet Key Exchange Protocol version 2 (IKEv2).  These attributes can be used for carrying the IPv4 address and IPv6 address of the Proxy-Call Session Control Function (P-CSCF).  When an IPsec gateway delivers these attributes to an IPsec client, the IPsec client can obtain the IPv4 and/or IPv6 address of the P-CSCF server located in the 3GPP network."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC7651;

  obs [label="obsoletes", shape=box];
  RFC7651 -> obs;

  updates [label="updates", shape=box];
  RFC7651 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC7651;

  cites [label="cites", shape=box];
  RFC7651 -> cites;
  RFC4303 [label="RFC4303
IP Encapsulating Security Payload (ESP)
2005", shape=ellipse, URL="RFC4303.html", target="_top", tooltip="This document describes an updated version of the Encapsulating Security Payload (ESP) protocol, which is designed to provide a mix of security services in IPv4 and IPv6.  ESP is used to provide confidentiality, data origin authentication, connectionless integrity, an anti-replay service (a form of partial sequence integrity), and limited traffic flow confidentiality.  This document obsoletes RFC 2406 (November 1998). [STANDARDS-TRACK]"];
  cites -> RFC4303;
  RFC5213 [label="RFC5213
Proxy Mobile IPv6
2008", shape=ellipse, URL="RFC5213.html", target="_top", tooltip="Network-based mobility management enables IP mobility for a host without requiring its participation in any mobility-related signaling.  The network is responsible for managing IP mobility on behalf of the host.  The mobility entities in the network are responsible for tracking the movements of the host and initiating the required mobility signaling on its behalf.  This specification describes a network-based mobility management protocol and is referred to as Proxy Mobile IPv6. [STANDARDS-TRACK]"];
  cites -> RFC5213;
  RFC7296 [label="RFC7296
Internet Key Exchange Protocol Version 2
(IKEv2)
2014", shape=ellipse, URL="RFC7296.html", target="_top", tooltip="This document describes version 2 of the Internet Key Exchange (IKE) protocol.  IKE is a component of IPsec used for performing mutual authentication and establishing and maintaining Security Associations (SAs).  This document obsoletes RFC 5996, and includes all of the errata for it.  It advances IKEv2 to be an Internet Standard."];
  cites -> RFC7296;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC5739 [label="RFC5739
IPv6 Configuration in Internet Key
Exchange Protocol Version 2 (IKEv2)
2010", shape=ellipse, URL="RFC5739.html", target="_top", tooltip="When Internet Key Exchange Protocol version 2 (IKEv2) is used for remote VPN access (client to VPN gateway), the gateway assigns the client an IP address from the internal network using IKEv2 configuration payloads.  The configuration payloads specified in RFC 4306 work well for IPv4 but make it difficult to use certain features of IPv6.  This document specifies new configuration attributes for IKEv2 that allows the VPN gateway to assign IPv6 prefixes to clients, enabling all features of IPv6 to be used with the client-gateway virtual link.  This document defines an Experimental Protocol for the Internet community."];
  cites -> RFC5739;
  RFC5844 [label="RFC5844
IPv4 Support for Proxy Mobile IPv6
2010", shape=ellipse, URL="RFC5844.html", target="_top", tooltip="This document specifies extensions to the Proxy Mobile IPv6 protocol for adding IPv4 protocol support.  The scope of IPv4 protocol support is two-fold: 1) enable IPv4 home address mobility support to the mobile node, and 2) allow the mobility entities in the Proxy Mobile IPv6 domain to exchange signaling messages over an IPv4 transport network. [STANDARDS-TRACK]"];
  cites -> RFC5844;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC7651;
}
