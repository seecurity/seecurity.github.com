digraph Flow {
  layout=twopi;
  root=RFC2941;
  overlap=false;
  RFC2941 [label="RFC2941
Telnet Authentication Option
2000", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document describes the authentication option to the telnet protocol as a generic method for negotiating an authentication type and mode including whether encryption should be used and if credentials should be forwarded. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2941;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2941 -> obs;
  RFC1416 [label="RFC1416
Telnet Authentication Option
1993", shape=ellipse, URL="RFC1416.html", target="_top", tooltip="This RFC 1416 replaces RFC 1409, which has an important typographical error in the example on page 6 (one occurance of REPLY should be IS).  This memo defines an Experimental Protocol for the Internet community."];
  obs -> RFC1416;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2941 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2941;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2941 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2941;
  RFC2943 [label="RFC2943
TELNET Authentication Using DSA
2000", shape=ellipse, URL="RFC2943.html", target="_top", tooltip="This document defines a telnet authentication mechanism using the Digital Signature Algorithm (DSA).  It relies on the Telnet Authentication Option. [STANDARDS-TRACK]"];
  RFC2943 -> rev_cites;
  RFC2944 [label="RFC2944
Telnet Authentication: SRP
2000", shape=ellipse, URL="RFC2944.html", target="_top", tooltip="This document specifies an authentication scheme for the Telnet protocol under the framework described in RFC 2941, using the Secure Remote Password Protocol (SRP) authentication mechanism. [STANDARDS-TRACK]"];
  RFC2944 -> rev_cites;
  RFC4248 [label="RFC4248
The telnet URI Scheme
2005", shape=ellipse, URL="RFC4248.html", target="_top", tooltip="This document specifies the telnet Uniform Resource Identifier (URI) scheme that was originally specified in RFC 1738.  The purpose of this document is to allow RFC 1738 to be made obsolete while keeping the information about the scheme on standards track. [STANDARDS-TRACK]"];
  RFC4248 -> rev_cites;
  RFC6270 [label="RFC6270
The 'tn3270' URI Scheme
2011", shape=ellipse, URL="RFC6270.html", target="_top", tooltip="This document is the specification of the 'tn3270' Uniform Resource Identifier (URI) scheme, which is used to designate the access to the resources available via Telnet 3270 mode (TN3270) and Telnet 3270 Enhanced mode (TN3270E).  It updates RFC 1041 and RFC 2355, which specify these protocols, and RFC 1738, which firstly mentioned this URI scheme without defining its syntax and semantics. [STANDARDS-TRACK]"];
  RFC6270 -> rev_cites;
