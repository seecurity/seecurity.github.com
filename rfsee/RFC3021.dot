  cites -> RFC2050;
  RFC1631 [label="RFC1631
The IP Network Address Translator (NAT)
1994", shape=ellipse, URL="RFC1631.html", target="_top", tooltip="This memo proposes another short-term solution, address reuse, that complements CIDR or even makes it unnecessary.  The address reuse solution is to place Network Address Translators (NAT) at the borders of stub domains.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1631;
