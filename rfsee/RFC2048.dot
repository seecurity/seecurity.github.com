  RFC2426 -> rev_cites;
  RFC2445 [label="RFC2445
Internet Calendaring and Scheduling Core
Object Specification (iCalendar)
1998", shape=ellipse, URL="RFC2445.html", target="_top", tooltip="This memo has been defined to provide the definition of a common format for openly exchanging calendaring and scheduling information across the Internet. [STANDARDS-TRACK]"];
  RFC2445 -> rev_cites;
  RFC2565 [label="RFC2565
Internet Printing Protocol/1.0: Encoding
and Transport
1999", shape=ellipse, URL="RFC2565.html", target="_top", tooltip="This document defines the rules for encoding IPP operations and IPP attributes into a new Internet mime media type called application/ipp.  This document also defines the rules for transporting over HTTP a message body whose Content-Type is application/ipp.  This document defines an Experimental protocol for the Internet community."];
  RFC2565 -> rev_cites;
  RFC2566 [label="RFC2566
Internet Printing Protocol/1.0: Model
and Semantics
1999", shape=ellipse, URL="RFC2566.html", target="_top", tooltip="This document describes a simplified model consisting of abstract objects, their attributes, and their operations that is independent of encoding and transport.  This document also addresses security, internationalization, and directory issues.  This memo defines an Experimental Protocol for the Internet community."];
  RFC2566 -> rev_cites;
  RFC2652 [label="RFC2652
MIME Object Definitions for the Common
Indexing Protocol (CIP)
1999", shape=ellipse, URL="RFC2652.html", target="_top", tooltip="This document describes the definitions of those objects as well as the methods and requirements needed to define a new index type. [STANDARDS-TRACK]"];
