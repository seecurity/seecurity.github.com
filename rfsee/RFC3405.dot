digraph Flow {
  layout=twopi;
  root=RFC3405;
  overlap=false;
  RFC3405 [label="RFC3405
Dynamic Delegation Discovery System
(DDDS) Part Five: URI.ARPA Assignment
Procedures
2002", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document is fifth in a series that is completely specified in Dynamic Delegation Discovery System (DDDS) Part One: The Comprehensive DDDS (RFC 3401).  It is very important to note that it is impossible to read and understand any document in this series without reading the others.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3405;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3405 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3405 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3405;
  RFC8958 [label="RFC8958
Updated Registration Rules for URI.ARPA
2020", shape=ellipse, URL="RFC8958.html", target="_top", tooltip="This document updates RFC 3405 by removing references to the IETF tree from the procedures for requesting that a URI scheme be inserted into the URI.ARPA zone."];
  RFC8958 -> updated_by;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3405 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3405;
  RFC3406 [label="RFC3406
Uniform Resource Names (URN) Namespace
Definition Mechanisms
2002", shape=ellipse, URL="RFC3406.html", target="_top", tooltip="This document lays out general definitions of and mechanisms for establishing Uniform Resource Names (URN) namespaces.  The URN WG has defined a syntax for URNs in RFC 2141, as well as some proposed mechanisms for their resolution and use in Internet applications in RFC 3401 and RFC 3405.  The whole rests on the concept of individual namespaces within the URN structure.  Apart from proof-of-concept namespaces, the use of existing identifiers in URNs has been discussed in RFC 2288.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  RFC3406 -> rev_cites;
  RFC5483 [label="RFC5483
ENUM Implementation Issues and
Experiences
2009", shape=ellipse, URL="RFC5483.html", target="_top", tooltip="This document captures experiences in implementing systems based on the ENUM protocol and experiences of ENUM data that have been created by others.  As such, it clarifies the ENUM and Dynamic Delegation Discovery System standards.  Its aim is to help others by reporting both what is out there and potential pitfalls in interpreting the set of documents that specify the ENUM protocol.  It does not revise the standards but is intended to provide technical input to future revisions of those documents.  This memo provides information for the Internet community."];
  RFC5483 -> rev_cites;
  RFC8958 [label="RFC8958
Updated Registration Rules for URI.ARPA
2020", shape=ellipse, URL="RFC8958.html", target="_top", tooltip="This document updates RFC 3405 by removing references to the IETF tree from the procedures for requesting that a URI scheme be inserted into the URI.ARPA zone."];
  RFC8958 -> rev_cites;
  RFC9676 [label="RFC9676
LEX: A Uniform Resource Name (URN)
Namespace for Sources of Law
2025", shape=ellipse, URL="RFC9676.html", target="_top", tooltip="This document describes LEX, a Uniform Resource Name (URN) namespace identifier that identifies, names, assigns, and manages persistent resources in the legal domain. This specification allows adoption of a common convention by multiple jurisdictions to facilitate ease of reference and access to resources in the legal domain. This specification is an Independent Submission to the RFC Series. It is not a standard and does not have the consensus of the IETF."];
  RFC9676 -> rev_cites;
}
