digraph Flow {
  layout=twopi;
  root=RFC3866;
  overlap=false;
  RFC3866 [label="RFC3866
Language Tags and Ranges in the
Lightweight Directory Access Protocol
(LDAP)
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="It is often desirable to be able to indicate the natural language associated with values held in a directory and to be able to query the directory for values which fulfill the user's language needs.  This document details the use of Language Tags and Ranges in the Lightweight Directory Access Protocol (LDAP). [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3866;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3866 -> obs;
  RFC2596 [label="RFC2596
Use of Language Codes in LDAP
1999", shape=ellipse, URL="RFC2596.html", target="_top", tooltip="This document describes how language codes are carried in LDAP and are to be interpreted by LDAP servers. [STANDARDS-TRACK]"];
  obs -> RFC2596;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3866 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3866;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3866 -> cites;
  RFC3377 [label="RFC3377
Lightweight Directory Access Protocol
(v3): Technical Specification
2002", shape=ellipse, URL="RFC3377.html", target="_top", tooltip="None"];
  cites -> RFC3377;
  RFC3066 [label="RFC3066
Tags for the Identification of Languages
2001", shape=ellipse, URL="RFC3066.html", target="_top", tooltip="This document describes a language tag for use in cases where it is desired to indicate the language used in an information object, how to register values for use in this language tag, and a construct for matching such language tags.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC3066;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2251 [label="RFC2251
Lightweight Directory Access Protocol
(v3)
1997", shape=ellipse, URL="RFC2251.html", target="_top", tooltip="The protocol described in this document is designed to provide access to directories supporting the X.500 models, while not incurring the resource requirements of the X.500 Directory Access Protocol (DAP). [STANDARDS-TRACK]"];
  cites -> RFC2251;
  RFC2234 [label="RFC2234
Augmented BNF for Syntax Specifications:
ABNF
1997", shape=ellipse, URL="RFC2234.html", target="_top", tooltip="In the early days of the Arpanet, each specification contained its own definition of ABNF.  This included the email specifications, RFC733 and then RFC822 which have come to be the common citations for defining ABNF.  The current document separates out that definition, to permit selective reference.  Predictably, it also provides some modifications and enhancements. [STANDARDS-TRACK]"];
  cites -> RFC2234;
  RFC3674 [label="RFC3674
Feature Discovery in Lightweight
Directory Access Protocol (LDAP)
2003", shape=ellipse, URL="RFC3674.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is an extensible protocol with numerous elective features.  This document introduces a general mechanism for discovery of elective features and extensions which cannot be discovered using existing mechanisms."];
  cites -> RFC3674;
  RFC3383 [label="RFC3383
Internet Assigned Numbers Authority
(IANA) Considerations for the
Lightweight Directory Access Protocol
(LDAP)
2002", shape=ellipse, URL="RFC3383.html", target="_top", tooltip="None"];
  cites -> RFC3383;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3866;
  RFC4521 [label="RFC4521
Considerations for Lightweight Directory
Access Protocol (LDAP) Extensions
2006", shape=ellipse, URL="RFC4521.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is extensible.  It provides mechanisms for adding new operations, extending existing operations, and expanding user and system schemas.  This document discusses considerations for designers of LDAP extensions.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  RFC4521 -> rev_cites;
