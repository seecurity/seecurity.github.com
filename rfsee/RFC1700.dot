  cites -> RFC1314;
  RFC1494 [label="RFC1494
Equivalences between 1988 X.400 and
RFC-822 Message Bodies
1993", shape=ellipse, URL="RFC1494.html", target="_top", tooltip="This document describes the content of the IANA MHS/MIME Equivalence table, and defines the initial configuration of this table.  Mappings for new MIME content-types and/or X.400 body part types should be registered with the IANA to minimize redundancy and promote interoperability. [STANDARDS-TRACK]"];
  cites -> RFC1494;
  RFC1345 [label="RFC1345
Character Mnemonics and Character Sets
1992", shape=ellipse, URL="RFC1345.html", target="_top", tooltip="This memo lists a selection of characters and their presence in some coded character sets.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1345;
  RFC1557 [label="RFC1557
Korean Character Encoding for Internet
Messages
1993", shape=ellipse, URL="RFC1557.html", target="_top", tooltip="This document describes the encoding method being used to represent Korean characters in both header and body part of the Internet mail messages [RFC822].  This encoding method was specified in 1991, and has since then been used.  It has now widely being used in Korean IP networks.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1557;
  RFC1468 [label="RFC1468
Japanese Character Encoding for Internet
Messages
