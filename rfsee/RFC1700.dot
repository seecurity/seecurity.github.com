  cites -> RFC1184;
  RFC1096 [label="RFC1096
Telnet X display location option
1989", shape=ellipse, URL="RFC1096.html", target="_top", tooltip="This RFC specifies a standard for the Internet community.  Hosts on the Internet that transmit the X display location within the Telnet protocol are expected to adopt and implement this standard."];
  cites -> RFC1096;
  RFC1408 [label="RFC1408
Telnet Environment Option
1993", shape=ellipse, URL="RFC1408.html", target="_top", tooltip="This document specifies a mechanism for passing environment information between a telnet client and server. [STANDARDS-TRACK]"];
  cites -> RFC1408;
  RFC1409 [label="RFC1409
Telnet Authentication Option
1993", shape=ellipse, URL="RFC1409.html", target="_top", tooltip="This memo defines an Experimental Protocol for the Internet community."];
  cites -> RFC1409;
  RFC1572 [label="RFC1572
Telnet Environment Option
1994", shape=ellipse, URL="RFC1572.html", target="_top", tooltip="This document specifies a mechanism for passing environment information between a telnet client and server.  Use of this mechanism enables a telnet user to propagate configuration information to a remote host when connecting. [STANDARDS-TRACK]"];
  cites -> RFC1572;
  RFC1647 [label="RFC1647
TN3270 Enhancements
1994", shape=ellipse, URL="RFC1647.html", target="_top", tooltip="This document describes a protocol that more fully supports 3270 devices than do the existing tn3270 practices. [STANDARDS-TRACK]"];
  cites -> RFC1647;
  RFC1034 [label="RFC1034
Domain names - concepts and facilities
1987", shape=ellipse, URL="RFC1034.html", target="_top", tooltip="This RFC is the revised basic definition of The Domain Name System.  It obsoletes RFC-882.  This memo describes the domain style names and their used for host address look up and electronic mail forwarding.  It discusses the clients and servers in the domain name system and the protocol used between them."];
  cites -> RFC1034;
  RFC1035 [label="RFC1035
Domain names - implementation and
specification
1987", shape=ellipse, URL="RFC1035.html", target="_top", tooltip="This RFC is the revised specification of the protocol and format used in the implementation of the Domain Name System.  It obsoletes RFC-883.  This memo documents the details of the domain name client - server communication."];
  cites -> RFC1035;
  RFC1183 [label="RFC1183
New DNS RR Definitions
1990", shape=ellipse, URL="RFC1183.html", target="_top", tooltip="This memo defines five new DNS types for experimental purposes.  This RFC describes an Experimental Protocol for the Internet community, and requests discussion and suggestions for improvements."];
  cites -> RFC1183;
  RFC1348 [label="RFC1348
DNS NSAP RRs
