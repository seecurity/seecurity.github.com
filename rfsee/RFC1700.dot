  cites -> RFC1639;
  RFC1485 [label="RFC1485
A String Representation of Distinguished
Names (OSI-DS 23 (v5))
1993", shape=ellipse, URL="RFC1485.html", target="_top", tooltip="When a distinguished name is communicated between to users not using a directory protocol (e.g., in a mail message), there is a need to have a user-oriented string representation of distinguished name. [STANDARDS-TRACK]"];
  cites -> RFC1485;
  RFC1357 [label="RFC1357
A Format for E-mailing Bibliographic
Records
1992", shape=ellipse, URL="RFC1357.html", target="_top", tooltip="This memo defines a format for E-mailing bibliographic records of technical reports.  It is intended to accelerate the dissemination of information about new Computer Science Technical Reports (CS-TR).  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1357;
  RFC1584 [label="RFC1584
Multicast Extensions to OSPF
1994", shape=ellipse, URL="RFC1584.html", target="_top", tooltip="This memo documents enhancements to the OSPF protocol enabling the routing of IP multicast datagrams. [STANDARDS-TRACK]"];
  cites -> RFC1584;
  RFC1585 [label="RFC1585
MOSPF: Analysis and Experience
1994", shape=ellipse, URL="RFC1585.html", target="_top", tooltip="This memo documents how the MOSPF protocol satisfies the requirements imposed on Internet routing protocols by Internet Engineering Task Force internet routing protocol standardization criteria ([RFC 1264]).  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1585;
  RFC1586 [label="RFC1586
Guidelines for Running OSPF Over Frame
Relay Networks
1994", shape=ellipse, URL="RFC1586.html", target="_top", tooltip="This memo specifies guidelines for implementors and users of the Open Shortest Path First (OSPF) routing protocol to bring about improvements in how the protocol runs over frame relay networks.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1586;
  RFC1587 [label="RFC1587
The OSPF NSSA Option
1994", shape=ellipse, URL="RFC1587.html", target="_top", tooltip="This document describes a new optional type of OSPF area, somewhat humorously referred to as a not-so-stubby area (or NSSA).  NSSAs are similar to the existing OSPF stub area configuration option but have the additional capability of importing AS external routes in a limited fashion. [STANDARDS-TRACK]"];
  cites -> RFC1587;
  RFC1521 [label="RFC1521
MIME (Multipurpose Internet Mail
Extensions) Part One: Mechanisms for
Specifying and Describing the Format of
Internet Message Bodies
1993", shape=ellipse, URL="RFC1521.html", target="_top", tooltip="This document redefines the format of message bodies to allow multi-part textual and non-textual message bodies to be represented and exchanged without loss of information.  This is based on earlier work documented in RFC 934 and STD 11, RFC 1049, but extends and revises that work. [STANDARDS-TRACK]"];
  cites -> RFC1521;
  RFC1036 [label="RFC1036
Standard for interchange of USENET
messages
