  RFC5873 [label="RFC5873
Pre-Authentication Support for the
Protocol for Carrying Authentication for
Network Access (PANA)
2010", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines an extension to the Protocol for Carrying Authentication for Network Access (PANA) for proactively establishing a PANA Security Association between a PANA Client in one access network and a PANA Authentication Agent in another access network to which the PANA Client may move.  This document defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC5873;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
