  RFC5873 -> cites;
  RFC5191 [label="RFC5191
Protocol for Carrying Authentication for
Network Access (PANA)
2008", shape=ellipse, URL="RFC5191.html", target="_top", tooltip="This document defines the Protocol for Carrying Authentication for Network Access (PANA), a network-layer transport for Extensible Authentication Protocol (EAP) to enable network access authentication between clients and access networks.  In EAP terms, PANA is a UDP-based EAP lower layer that runs between the EAP peer and the EAP authenticator. [STANDARDS-TRACK]"];
  cites -> RFC5191;
  RFC5836 [label="RFC5836
Extensible Authentication Protocol (EAP)
Early Authentication Problem Statement
2010", shape=ellipse, URL="RFC5836.html", target="_top", tooltip="Extensible Authentication Protocol (EAP) early authentication may be defined as the use of EAP by a mobile device to establish authenticated keying material on a target attachment point prior to its arrival.  This document discusses the EAP early authentication problem in detail.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  cites -> RFC5836;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC5872 [label="RFC5872
IANA Rules for the Protocol for Carrying
Authentication for Network Access (PANA)
2010", shape=ellipse, URL="RFC5872.html", target="_top", tooltip="This document relaxes the IANA rules for the Protocol for Carrying Authentication for Network Access (PANA). [STANDARDS-TRACK]"];
  cites -> RFC5872;

