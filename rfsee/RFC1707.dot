digraph Flow {
  layout=twopi;
  root=RFC1707;
  overlap=false;
  RFC1707 [label="RFC1707
CATNIP: Common Architecture for the
Internet
1994", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document was submitted to the IETF IPng area in response to RFC 1550.  This paper describes a common architecture for the network layer protocol.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1707;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1707 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC1707 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC1707;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC1707 -> cites;
  RFC1191 [label="RFC1191
Path MTU discovery
1990", shape=ellipse, URL="RFC1191.html", target="_top", tooltip="This memo describes a technique for dynamically discovering the maximum transmission unit (MTU) of an arbitrary internet path.  It specifies a small change to the way routers generate one type of ICMP message.  For a path that passes through a router that has not been so changed, this technique might not discover the correct Path MTU, but it will always choose a Path MTU as accurate as, and in many cases more accurate than, the Path MTU that would be chosen by current practice. [STANDARDS-TRACK]"];
  cites -> RFC1191;
  RFC1234 [label="RFC1234
Tunneling IPX traffic through IP
networks
1991", shape=ellipse, URL="RFC1234.html", target="_top", tooltip="This memo describes a method of encapsulating IPX datagrams within UDP packets so that IPX traffic can travel across an IP internet. [STANDARDS-TRACK] This memo defines objects for managing DS1 Interface objects for use with the SNMP protocol. [STANDARDS-TRACK]"];
  cites -> RFC1234;
  RFC1247 [label="RFC1247
OSPF Version 2
1991", shape=ellipse, URL="RFC1247.html", target="_top", tooltip="This memo documents version 2 of the OSPF protocol.  OSPF is a link- state based routing protocol. [STANDARDS-TRACK]"];
  cites -> RFC1247;
  RFC1287 [label="RFC1287
Towards the Future Internet Architecture
1991", shape=ellipse, URL="RFC1287.html", target="_top", tooltip="This informational RFC discusses important directions for possible future evolution of the Internet architecture, and suggests steps towards the desired goals.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1287;
  RFC1335 [label="RFC1335
A Two-Tier Address Structure for the
Internet: A Solution to the Problem of
Address Space Exhaustion
1992", shape=ellipse, URL="RFC1335.html", target="_top", tooltip="This RFC presents a solution to problem of address space exhaustion in the Internet.  It proposes a two-tier address structure for the Internet.  This is an idea paper and discussion is strongly encouraged.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1335;
  RFC1338 [label="RFC1338
Supernetting: an Address Assignment and
Aggregation Strategy
1992", shape=ellipse, URL="RFC1338.html", target="_top", tooltip="This memo discusses strategies for address assignment of the existing IP address space with a view to conserve the address space and stem the explosive growth of routing tables in default-route-free routers run by transit routing domain providers.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1338;
  RFC1347 [label="RFC1347
TCP and UDP with Bigger Addresses
(TUBA), A Simple Proposal for Internet
Addressing and Routing
1992", shape=ellipse, URL="RFC1347.html", target="_top", tooltip="This paper describes a simple proposal which provides a long-term solution to Internet addressing, routing, and scaling.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1347;
  RFC1466 [label="RFC1466
Guidelines for Management of IP Address
Space
1993", shape=ellipse, URL="RFC1466.html", target="_top", tooltip="This document proposes a plan which will forward the implementation of RFC 1174 and which defines the allocation and assignment of the network number space.  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1466;
  RFC1475 [label="RFC1475
TP/IX: The Next Internet
1993", shape=ellipse, URL="RFC1475.html", target="_top", tooltip="This memo presents the specification for version 7 of the Internet Protocol, as well as version 7 of the TCP and the user datagram protocol.  This memo defines an Experimental Protocol for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1475;
  RFC1476 [label="RFC1476
RAP: Internet Route Access Protocol
1993", shape=ellipse, URL="RFC1476.html", target="_top", tooltip="This RFC describes an open distance vector routing protocol for use at all levels of the internet, from isolated LANs to the major routers of an international commercial network provider.  This memo defines an Experimental Protocol for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1476;
  RFC1561 [label="RFC1561
Use of ISO CLNP in TUBA Environments
1993", shape=ellipse, URL="RFC1561.html", target="_top", tooltip="This memo specifies a profile of the ISO/IEC 8473 Connectionless-mode Network Layer Protocol for use in conjunction with RFC 1347, TCP/UDP over Bigger Addresses.  It describes the use of CLNP to provide the lower-level service expected by Transmission Control Protocol and User Datagram Protocol.  This memo defines an Experimental Protocol for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1561;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC1707;
  RFC6294 [label="RFC6294
Survey of Proposed Use Cases for the
IPv6 Flow Label
2011", shape=ellipse, URL="RFC6294.html", target="_top", tooltip="The IPv6 protocol includes a flow label in every packet header, but this field is not used in practice.  This paper describes the flow label standard and discusses the implementation issues that it raises.  It then describes various published proposals for using the flow label and shows that most of them are inconsistent with the standard.  Methods to address this problem are briefly reviewed.  We also question whether the standard should be revised.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  RFC6294 -> rev_cites;
  RFC6328 [label="RFC6328
IANA Considerations for Network Layer
Protocol Identifiers
2011", shape=ellipse, URL="RFC6328.html", target="_top", tooltip="Some protocols being developed or extended by the IETF make use of the ISO/IEC (International Organization for Standardization / International Electrotechnical Commission) Network Layer Protocol Identifier (NLPID).  This document provides NLPID IANA considerations.  This memo documents an Internet Best Current Practice."];
  RFC6328 -> rev_cites;
}
