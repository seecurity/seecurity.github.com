  RFC8478 [label="RFC8478
Zstandard Compression and the
application/zstd Media Type
2018", shape=ellipse, fillcolor="#e8f0ff", tooltip="Zstandard, or zstd (pronounced zee standard), is a data compression mechanism. This document describes the mechanism and registers a media type and content encoding to be used when transporting zstd-compressed content via Multipurpose Internet Mail Extensions (MIME). Despite use of the word standard as part of its name, readers are advised that this document is not an Internet Standards Track specification; it is being published for informational purposes only.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8478;
  RFC8878 [label="RFC8878
Zstandard Compression and the
'application/zstd' Media Type
2021", shape=ellipse, URL="RFC8878.html", target="_top", tooltip="Zstandard, or zstd (pronounced zee standard), is a lossless data compression mechanism. This document describes the mechanism and registers a media type, content encoding, and a structured syntax suffix to be used when transporting zstd-compressed content via MIME. Despite use of the word standard as part of Zstandard, readers are advised that this document is not an Internet Standards Track specification; it is being published for informational purposes only. This document replaces and obsoletes RFC 8478."];
  RFC8878 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8478 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8478 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC8478;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC8478 -> cites;
  RFC1952 [label="RFC1952
GZIP file format specification version
4.3
1996", shape=ellipse, URL="RFC1952.html", target="_top", tooltip="This specification defines a lossless compressed data format that is compatible with the widely used GZIP utility.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1952;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC8478;
  RFC8879 [label="RFC8879
TLS Certificate Compression
2020", shape=ellipse, URL="RFC8879.html", target="_top", tooltip="In TLS handshakes, certificate chains often take up the majority of the bytes transmitted. This document describes how certificate chains can be compressed to reduce the amount of data transmitted and avoid some round trips."];
  RFC8879 -> rev_cites;
