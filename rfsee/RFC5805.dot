digraph Flow {
  layout=twopi;
  root=RFC5805;
  overlap=false;
  RFC5805 [label="RFC5805
Lightweight Directory Access Protocol
(LDAP) Transactions
2010", shape=ellipse, fillcolor="#e8f0ff", tooltip="Lightweight Directory Access Protocol (LDAP) update operations, such as Add, Delete, and Modify operations, have atomic, consistency, isolation, durability (ACID) properties.  Each of these update operations act upon an entry.  It is often desirable to update two or more entries in a single unit of interaction, a transaction.  Transactions are necessary to support a number of applications including resource provisioning.  This document extends LDAP to support transactions.  This document defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC5805;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC5805 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC5805 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC5805;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC5805 -> cites;
  RFC4510 [label="RFC4510
Lightweight Directory Access Protocol
(LDAP): Technical Specification Road Map
2006", shape=ellipse, URL="RFC4510.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is an Internet protocol for accessing distributed directory services that act in accordance with X.500 data and service models.  This document provides a road map of the LDAP Technical Specification. [STANDARDS-TRACK]"];
  cites -> RFC4510;
  RFC4511 [label="RFC4511
Lightweight Directory Access Protocol
(LDAP): The Protocol
2006", shape=ellipse, URL="RFC4511.html", target="_top", tooltip="This document describes the protocol elements, along with their semantics and encodings, of the Lightweight Directory Access Protocol (LDAP).  LDAP provides access to distributed directory services that act in accordance with X.500 data and service models.  These protocol elements are based on those described in the X.500 Directory Access Protocol (DAP). [STANDARDS-TRACK]"];
  cites -> RFC4511;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC3062 [label="RFC3062
LDAP Password Modify Extended Operation
2001", shape=ellipse, URL="RFC3062.html", target="_top", tooltip="This document describes an LDAP extended operation to allow modification of user passwords which is not dependent upon the form of the authentication identity nor the password storage mechanism used. [STANDARDS-TRACK]"];
  cites -> RFC3062;
  RFC4512 [label="RFC4512
Lightweight Directory Access Protocol
(LDAP): Directory Information Models
2006", shape=ellipse, URL="RFC4512.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is an Internet protocol for accessing distributed directory services that act in accordance with X.500 data and service models.  This document describes the X.500 Directory Information Models, as used in LDAP. [STANDARDS-TRACK]"];
  cites -> RFC4512;
  RFC4528 [label="RFC4528
Lightweight Directory Access Protocol
(LDAP) Assertion Control
2006", shape=ellipse, URL="RFC4528.html", target="_top", tooltip="This document defines the Lightweight Directory Access Protocol (LDAP) Assertion Control, which allows a client to specify that a directory operation should only be processed if an assertion applied to the target entry of the operation is true.  It can be used to construct test and set, test and clear, and other conditional operations. [STANDARDS-TRACK]"];
  cites -> RFC4528;
  RFC3296 [label="RFC3296
Named Subordinate References in
Lightweight Directory Access Protocol
(LDAP) Directories
2002", shape=ellipse, URL="RFC3296.html", target="_top", tooltip="This document details schema and protocol elements for representing and managing named subordinate references in Lightweight Directory Access Protocol (LDAP) Directories. [STANDARDS-TRACK]"];
  cites -> RFC3296;
  RFC4370 [label="RFC4370
Lightweight Directory Access Protocol
(LDAP) Proxied Authorization Control
2006", shape=ellipse, URL="RFC4370.html", target="_top", tooltip="This document defines the Lightweight Directory Access Protocol (LDAP) Proxy Authorization Control.  The Proxy Authorization Control allows a client to request that an operation be processed under a provided authorization identity instead of under the current authorization identity associated with the connection. [STANDARDS-TRACK]"];
  cites -> RFC4370;
  RFC4527 [label="RFC4527
Lightweight Directory Access Protocol
(LDAP) Read Entry Controls
2006", shape=ellipse, URL="RFC4527.html", target="_top", tooltip="This document specifies an extension to the Lightweight Directory Access Protocol (LDAP) to allow the client to read the target entry of an update operation.  The client may request to read the entry before and/or after the modifications are applied.  These reads are done as an atomic part of the update operation. [STANDARDS-TRACK]"];
  cites -> RFC4527;
  RFC4520 [label="RFC4520
Internet Assigned Numbers Authority
(IANA) Considerations for the
Lightweight Directory Access Protocol
(LDAP)
2006", shape=ellipse, URL="RFC4520.html", target="_top", tooltip="This document provides procedures for registering extensible elements of the Lightweight Directory Access Protocol (LDAP).  The document also provides guidelines to the Internet Assigned Numbers Authority (IANA) describing conditions under which new values can be assigned.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC4520;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC5805;
}
