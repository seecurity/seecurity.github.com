digraph Flow {
  layout=twopi;
  root=RFC4533;
  overlap=false;
  RFC4533 [label="RFC4533
The Lightweight Directory Access
Protocol (LDAP) Content Synchronization
Operation
2006", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification describes the Lightweight Directory Access Protocol (LDAP) Content Synchronization Operation.  The operation allows a client to maintain a copy of a fragment of the Directory Information Tree (DIT).  It supports both polling for changes and listening for changes.  The operation is defined as an extension of the LDAP Search Operation.  This memo defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC4533;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC4533 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC4533 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC4533;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC4533 -> cites;
  RFC4510 [label="RFC4510
Lightweight Directory Access Protocol
(LDAP): Technical Specification Road Map
2006", shape=ellipse, URL="RFC4510.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is an Internet protocol for accessing distributed directory services that act in accordance with X.500 data and service models.  This document provides a road map of the LDAP Technical Specification. [STANDARDS-TRACK]"];
  cites -> RFC4510;
  RFC4511 [label="RFC4511
Lightweight Directory Access Protocol
(LDAP): The Protocol
2006", shape=ellipse, URL="RFC4511.html", target="_top", tooltip="This document describes the protocol elements, along with their semantics and encodings, of the Lightweight Directory Access Protocol (LDAP).  LDAP provides access to distributed directory services that act in accordance with X.500 data and service models.  These protocol elements are based on those described in the X.500 Directory Access Protocol (DAP). [STANDARDS-TRACK]"];
  cites -> RFC4511;
  RFC4530 [label="RFC4530
Lightweight Directory Access Protocol
(LDAP) entryUUID Operational Attribute
2006", shape=ellipse, URL="RFC4530.html", target="_top", tooltip="This document describes the LDAP/X.500 \'entryUUID' operational attribute and associated matching rules and syntax.  The attribute holds a server-assigned Universally Unique Identifier (UUID) for the object.  Directory clients may use this attribute to distinguish objects identified by a distinguished name or to locate an object after renaming. [STANDARDS-TRACK]"];
  cites -> RFC4530;
  RFC3909 [label="RFC3909
Lightweight Directory Access Protocol
(LDAP) Cancel Operation
2004", shape=ellipse, URL="RFC3909.html", target="_top", tooltip="This specification describes a Lightweight Directory Access Protocol (LDAP) extended operation to cancel (or abandon) an outstanding operation.  Unlike the LDAP Abandon operation, but like the X.511 Directory Access Protocol (DAP) Abandon operation, this operation has a response which provides an indication of its outcome. [STANDARDS-TRACK]"];
  cites -> RFC3909;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC4512 [label="RFC4512
Lightweight Directory Access Protocol
(LDAP): Directory Information Models
2006", shape=ellipse, URL="RFC4512.html", target="_top", tooltip="The Lightweight Directory Access Protocol (LDAP) is an Internet protocol for accessing distributed directory services that act in accordance with X.500 data and service models.  This document describes the X.500 Directory Information Models, as used in LDAP. [STANDARDS-TRACK]"];
  cites -> RFC4512;
  RFC3296 [label="RFC3296
Named Subordinate References in
Lightweight Directory Access Protocol
(LDAP) Directories
2002", shape=ellipse, URL="RFC3296.html", target="_top", tooltip="This document details schema and protocol elements for representing and managing named subordinate references in Lightweight Directory Access Protocol (LDAP) Directories. [STANDARDS-TRACK]"];
  cites -> RFC3296;
  RFC3671 [label="RFC3671
Collective Attributes in the Lightweight
Directory Access Protocol (LDAP)
2003", shape=ellipse, URL="RFC3671.html", target="_top", tooltip="X.500 collective attributes allow common characteristics to be shared between collections of entries.  This document summarizes the X.500 information model for collective attributes and describes use of collective attributes in LDAP (Lightweight Directory Access Protocol).  This document provides schema definitions for collective attributes for use in LDAP."];
  cites -> RFC3671;
  RFC3672 [label="RFC3672
Subentries in the Lightweight Directory
Access Protocol (LDAP)
2003", shape=ellipse, URL="RFC3672.html", target="_top", tooltip="In X.500 directories, subentries are special entries used to hold information associated with a subtree or subtree refinement.  This document adapts X.500 subentries mechanisms for use with the Lightweight Directory Access Protocol (LDAP)."];
  cites -> RFC3672;
  RFC4520 [label="RFC4520
Internet Assigned Numbers Authority
(IANA) Considerations for the
Lightweight Directory Access Protocol
(LDAP)
2006", shape=ellipse, URL="RFC4520.html", target="_top", tooltip="This document provides procedures for registering extensible elements of the Lightweight Directory Access Protocol (LDAP).  The document also provides guidelines to the Internet Assigned Numbers Authority (IANA) describing conditions under which new values can be assigned.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC4520;
  RFC3928 [label="RFC3928
Lightweight Directory Access Protocol
(LDAP) Client Update Protocol (LCUP)
2004", shape=ellipse, URL="RFC3928.html", target="_top", tooltip="This document defines the Lightweight Directory Access Protocol (LDAP) Client Update Protocol (LCUP).  The protocol is intended to allow an LDAP client to synchronize with the content of a directory information tree (DIT) stored by an LDAP server and to be notified about the changes to that content. [STANDARDS-TRACK]"];
  cites -> RFC3928;
  RFC2251 [label="RFC2251
Lightweight Directory Access Protocol
(v3)
1997", shape=ellipse, URL="RFC2251.html", target="_top", tooltip="The protocol described in this document is designed to provide access to directories supporting the X.500 models, while not incurring the resource requirements of the X.500 Directory Access Protocol (DAP). [STANDARDS-TRACK]"];
  cites -> RFC2251;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC4533;
}
