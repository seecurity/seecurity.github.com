digraph Flow {
  layout=twopi;
  root=RFC2806;
  overlap=false;
  RFC2806 [label="RFC2806
URLs for Telephone Calls
2000", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document specifies URL (Uniform Resource Locator) schemes tel, fax and modem for specifying the location of a terminal in the phone network and the connection types (modes of operation) that can be used to connect to that entity. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2806;
  RFC3966 [label="RFC3966
The tel URI for Telephone Numbers
2004", shape=ellipse, URL="RFC3966.html", target="_top", tooltip="This document specifies the URI (Uniform Resource Identifier) scheme tel.  The tel URI describes resources identified by telephone numbers.  This document obsoletes RFC 2806. [STANDARDS-TRACK]"];
  RFC3966 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2806 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2806 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2806;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2806 -> cites;
  RFC2303 [label="RFC2303
Minimal PSTN address format in Internet
Mail
1998", shape=ellipse, URL="RFC2303.html", target="_top", tooltip="This memo describes the MINIMAL addressing method to encode PSTN addresses into e-mail addresses and the standard extension mechanism to allow definition of further standard elements. [STANDARDS-TRACK]"];
  cites -> RFC2303;
  RFC2304 [label="RFC2304
Minimal FAX address format in Internet
Mail
1998", shape=ellipse, URL="RFC2304.html", target="_top", tooltip="This memo describes the MINIMAL addressing method and standard extensions to encode FAX addresses in e-mail addresses. [STANDARDS-TRACK]"];
  cites -> RFC2304;
  RFC2396 [label="RFC2396
Uniform Resource Identifiers (URI):
Generic Syntax
1998", shape=ellipse, URL="RFC2396.html", target="_top", tooltip="This document defines a grammar that is a superset of all valid URI, such that an implementation can parse the common components of a URI reference without knowing the scheme-specific requirements of every possible identifier type. [STANDARDS-TRACK]"];
  cites -> RFC2396;
  RFC2543 [label="RFC2543
SIP: Session Initiation Protocol
1999", shape=ellipse, URL="RFC2543.html", target="_top", tooltip="The Session Initiation Protocol (SIP) is an application-layer control (signaling) protocol for creating, modifying and terminating sessions with one or more participants. [STANDARDS-TRACK]"];
  cites -> RFC2543;
  RFC2234 [label="RFC2234
Augmented BNF for Syntax Specifications:
ABNF
1997", shape=ellipse, URL="RFC2234.html", target="_top", tooltip="In the early days of the Arpanet, each specification contained its own definition of ABNF.  This included the email specifications, RFC733 and then RFC822 which have come to be the common citations for defining ABNF.  The current document separates out that definition, to permit selective reference.  Predictably, it also provides some modifications and enhancements. [STANDARDS-TRACK]"];
  cites -> RFC2234;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC1035 [label="RFC1035
Domain names - implementation and
specification
1987", shape=ellipse, URL="RFC1035.html", target="_top", tooltip="This RFC is the revised specification of the protocol and format used in the implementation of the Domain Name System.  It obsoletes RFC-883.  This memo documents the details of the domain name client - server communication."];
  cites -> RFC1035;
  RFC1738 [label="RFC1738
Uniform Resource Locators (URL)
1994", shape=ellipse, URL="RFC1738.html", target="_top", tooltip="This document specifies a Uniform Resource Locator (URL), the syntax and semantics of formalized information for location and access of resources via the Internet. [STANDARDS-TRACK]"];
  cites -> RFC1738;
  RFC2048 [label="RFC2048
Multipurpose Internet Mail Extensions
(MIME) Part Four: Registration
Procedures
1996", shape=ellipse, URL="RFC2048.html", target="_top", tooltip="This set of documents, collectively called the Multipurpose Internet Mail Extensions, or MIME, redefines the format of messages.  This fourth document, RFC 2048, specifies various IANA registration procedures for some MIME facilities.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2048;
  RFC1866 [label="RFC1866
Hypertext Markup Language - 2.0
1995", shape=ellipse, URL="RFC1866.html", target="_top", tooltip="This document defines a HTML 2.0 (to distinguish it from the previous informal specifications). [STANDARDS-TRACK]"];
  cites -> RFC1866;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2806;
  RFC3939 [label="RFC3939
Calling Line Identification for Voice
Mail Messages
2004", shape=ellipse, URL="RFC3939.html", target="_top", tooltip="This document describes a method for identifying the originating calling party in the headers of a stored voice mail message.  Two new header fields are defined for this purpose: Caller_ID and Called_Name.  Caller_id is used to store sufficient information for the recipient to callback, or reply to, the sender of the message.  Caller-name provides the name of the person sending the message. [STANDARDS-TRACK]"];
  RFC3939 -> rev_cites;
  RFC3966 [label="RFC3966
The tel URI for Telephone Numbers
2004", shape=ellipse, URL="RFC3966.html", target="_top", tooltip="This document specifies the URI (Uniform Resource Identifier) scheme tel.  The tel URI describes resources identified by telephone numbers.  This document obsoletes RFC 2806. [STANDARDS-TRACK]"];
  RFC3966 -> rev_cites;
}
