  RFC3034 [label="RFC3034
Use of Label Switching on Frame Relay
Networks Specification
2001", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the model and generic mechanisms for Multiprotocol Label Switching on Frame Relay networks. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3034;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3034 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3034 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3034;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
