  cites -> RFC5890;
  RFC7481 [label="RFC7481
Security Services for the Registration
Data Access Protocol (RDAP)
2015", shape=ellipse, URL="RFC7481.html", target="_top", tooltip="The Registration Data Access Protocol (RDAP) provides RESTful web services to retrieve registration metadata from Domain Name and Regional Internet Registries.  This document describes information security services, including access control, authentication, authorization, availability, data confidentiality, and data integrity for RDAP."];
  cites -> RFC7481;
  RFC8605 [label="RFC8605
vCard Format Extensions: ICANN
Extensions for the Registration Data
Access Protocol (RDAP)
2019", shape=ellipse, URL="RFC8605.html", target="_top", tooltip="This document defines extensions to the vCard data format for representing and exchanging contact information used to implement the Internet Corporation for Assigned Names and Numbers (ICANN) operational profile for the Registration Data Access Protocol (RDAP).  The property and parameter defined here are used to add values to RDAP responses that are consistent with ICANN policies."];
