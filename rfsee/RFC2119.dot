  RFC7371 -> rev_cites;
  RFC7372 [label="RFC7372
Email Authentication Status Codes
2014", shape=ellipse, URL="RFC7372.html", target="_top", tooltip="This document registers code points to allow status codes to be returned to an email client to indicate that a message is being rejected or deferred specifically because of email authentication failures. This document updates RFC 7208, since some of the code points registered replace the ones recommended for use in that document."];
