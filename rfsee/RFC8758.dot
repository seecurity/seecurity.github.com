  RFC8758 [label="RFC8758
Deprecating RC4 in Secure Shell (SSH)
2020", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document deprecates RC4 in Secure Shell (SSH).  Therefore, this document formally moves RFC 4345 to Historic status.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
