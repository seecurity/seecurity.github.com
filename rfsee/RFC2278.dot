  RFC2278 [label="RFC2278
IANA Charset Registration Procedures
1998", shape=ellipse, fillcolor="#e8f0ff", tooltip="MIME [RFC-2045, RFC-2046, RFC-2047, RFC-2184] and various other modern Internet protocols are capable of using many different charsets.  This in turn means that the ability to label different charsets is essential.  This registration procedure exists solely to associate a specific name or names with a given charset and to give an indication of whether or not a given charset can be used in MIME text objects.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC2278;
  RFC2978 [label="RFC2978
IANA Charset Registration Procedures
2000", shape=ellipse, URL="RFC2978.html", target="_top", tooltip="Multipurpose Internet Mail Extensions (MIME) and various other Internet protocols are capable of using many different charsets.  This in turn means that the ability to label different charsets is essential.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  RFC2978 -> obs_by;

  obs [label="obsoletes", shape=box];
  RFC2278 -> obs;

  updates [label="updates", shape=box];
  RFC2278 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC2278;

