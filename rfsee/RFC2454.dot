  RFC2454 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2454;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2454 -> cites;
  RFC2274 [label="RFC2274
User-based Security Model (USM) for
version 3 of the Simple Network
Management Protocol (SNMPv3)
1998", shape=ellipse, URL="RFC2274.html", target="_top", tooltip="This document describes the User-based Security Model (USM) for SNMP version 3 for use in the SNMP architecture [RFC2261].  It defines the Elements of Procedure for providing SNMP message level security.  This document also includes a MIB for remotely monitoring/managing the configuration parameters for this Security Model. [STANDARDS-TRACK]"];
  cites -> RFC2274;
  RFC2275 [label="RFC2275
View-based Access Control Model (VACM)
for the Simple Network Management
Protocol (SNMP)
1998", shape=ellipse, URL="RFC2275.html", target="_top", tooltip="This document describes the View-based Access Control Model for use in the SNMP architecture [RFC2261].  It defines the Elements of Procedure for controlling access to management information.  This document also includes a MIB for remotely managing the configuration parameters for the View-based Access Control Model. [STANDARDS-TRACK]"];
  cites -> RFC2275;

