  RFC4638 [label="RFC4638
Accommodating a Maximum Transit
Unit/Maximum Receive Unit (MTU/MRU)
Greater Than 1492 in the Point-to-Point
Protocol over Ethernet (PPPoE)
2006", shape=ellipse, fillcolor="#e8f0ff", tooltip="The Point-to-Point Protocol over Ethernet (PPPoE), as described in RFC 2516, mandates a maximum negotiated Maximum Receive Unit (MRU) of 1492.  This document outlines a solution that relaxes this restriction and allows a maximum negotiated MRU greater than 1492 to minimize fragmentation in next-generation broadband networks.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC4638;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC4638 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
