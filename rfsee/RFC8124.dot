  RFC8124 [label="RFC8124
The Session Description Protocol (SDP)
WebSocket Connection URI Attribute
2017", shape=ellipse, fillcolor="#e8f0ff", tooltip="The WebSocket protocol enables bidirectional real-time communication between clients and servers in web-based applications.  This document specifies extensions to Session Description Protocol (SDP) for application protocols using WebSocket as a transport.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
