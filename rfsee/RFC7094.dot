  RFC7094 [label="RFC7094
Architectural Considerations of IP
Anycast
2014", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo discusses architectural implications of IP anycast and provides some historical analysis of anycast use by various IETF protocols.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

