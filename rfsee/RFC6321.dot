  RFC6321 [label="RFC6321
xCal: The XML Format for iCalendar
2011", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification defines xCal, an XML format for iCalendar data. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC6321;

