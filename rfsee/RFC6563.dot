  RFC1886 [label="RFC1886
DNS Extensions to support IP version 6
1995", shape=ellipse, URL="RFC1886.html", target="_top", tooltip="This document defines the changes that need to be made to the Domain Name System to support hosts running IP version 6 (IPv6). [STANDARDS-TRACK]"];
  cites -> RFC1886;
  RFC3596 [label="RFC3596
DNS Extensions to Support IP Version 6
2003", shape=ellipse, URL="RFC3596.html", target="_top", tooltip="This document defines the changes that need to be made to the Domain Name System (DNS) to support hosts running IP version 6 (IPv6).  The changes include a resource record type to store an IPv6 address, a domain to support lookups based on an IPv6 address, and updated definitions of existing query types that return Internet addresses as part of additional section processing.  The extensions are designed to be compatible with existing applications and, in particular, DNS implementations themselves. [STANDARDS-TRACK]"];
  cites -> RFC3596;
  RFC2874 [label="RFC2874
DNS Extensions to Support IPv6 Address
Aggregation and Renumbering
2000", shape=ellipse, URL="RFC2874.html", target="_top", tooltip="This document defines changes to the Domain Name System to support renumberable and aggregatable IPv6 addressing. [STANDARDS-TRACK]"];
  cites -> RFC2874;
