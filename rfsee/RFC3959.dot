  RFC3959 [label="RFC3959
The Early Session Disposition Type for
the Session Initiation Protocol (SIP)
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines a new disposition type (early-session) for the Content-Disposition header field in the Session Initiation Protocol (SIP).  The treatment of early-session bodies is similar to the treatment of session bodies.  That is, they follow the offer/answer model.  Their only difference is that session descriptions whose disposition type is early-session are used to establish early media sessions within early dialogs, as opposed to regular sessions within regular dialogs. [STANDARDS-TRACK]"]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC3959;

  obs [label="obsoletes", shape=box];
  RFC3959 -> obs;

  updates [label="updates", shape=box];
  RFC3959 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC3959;

