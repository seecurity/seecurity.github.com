digraph Flow {
  layout=twopi;
  root=RFC3700;
  overlap=false;
  RFC3700 [label="RFC3700
Internet Official Protocol Standards
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo contains a snapshot of the state of standardization of protocols used in the Internet as of July 22, 2004.  It lists official protocol standards and Best Current Practice RFCs; it is not a complete index to the RFC series.  The latest version of this memo is designated STD 1. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3700;
  RFC5000 [label="RFC5000
Internet Official Protocol Standards
2008", shape=ellipse, URL="RFC5000.html", target="_top", tooltip="This document is published by the RFC Editor to provide a summary of the current standards protocols (as of 18 February 2008). It lists those official protocol standards, Best Current Practice, and Experimental RFCs that have not been obsoleted; it is not a complete index to the RFC series. Newly published RFCs and RFCs whose status has changed are starred. For an up-to-date list, see http://www.rfc-editor.org/rfcxx00.html, which is updated daily. This memo provides information for the Internet community."];
  RFC5000 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3700 -> obs;
  RFC3600 [label="RFC3600
Internet Official Protocol Standards
2003", shape=ellipse, URL="RFC3600.html", target="_top", tooltip="This memo contains a snapshot of the state of standardization of protocols used in the Internet as of October 2, 2003.  It lists official protocol standards and Best Current Practice RFCs; it is not a complete index to the RFC series.  The latest version of this memo is designated STD 1."];
  obs -> RFC3600;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3700 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3700;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3700 -> cites;
  RFC1700 [label="RFC1700
Assigned Numbers
1994", shape=ellipse, URL="RFC1700.html", target="_top", tooltip="This RFC is a snapshot of the ongoing process of the assignment of protocol parameters for the Internet protocol suite.  To make the current information readily available the assignments are kept up-to- date in a set of online text files.  This memo is a status report on the parameters (i.e., numbers and keywords) used in protocols in the Internet community."];
  cites -> RFC1700;
  RFC3232 [label="RFC3232
Assigned Numbers: RFC 1700 is Replaced
by an On-line Database
2002", shape=ellipse, URL="RFC3232.html", target="_top", tooltip="This memo obsoletes RFC 1700 (STD 2) Assigned Numbers, which contained an October 1994 snapshot of assigned Internet protocol parameters.  This memo provides information for the Internet community."];
  cites -> RFC3232;
  RFC1812 [label="RFC1812
Requirements for IP Version 4 Routers
1995", shape=ellipse, URL="RFC1812.html", target="_top", tooltip="This memo defines and discusses requirements for devices that perform the network layer forwarding function of the Internet protocol suite. [STANDARDS-TRACK]"];
  cites -> RFC1812;
  RFC1305 [label="RFC1305
Network Time Protocol (Version 3)
Specification, Implementation and
Analysis
1992", shape=ellipse, URL="RFC1305.html", target="_top", tooltip="This document describes the Network Time Protocol (NTP), specifies its formal structure and summarizes information useful for its implementation."];
  cites -> RFC1305;
  RFC3638 [label="RFC3638
Applicability Statement for
Reclassification of RFC 1643 to Historic
Status
2003", shape=ellipse, URL="RFC3638.html", target="_top", tooltip="This memo recommends that RFC 1643 be reclassified as an Historic document and provides the supporting motivation for that recommendation."];
  cites -> RFC3638;

