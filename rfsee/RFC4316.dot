  RFC4316 [label="RFC4316
Datatypes for Web Distributed Authoring
and Versioning (WebDAV) Properties
2005", shape=ellipse, fillcolor="#e8f0ff", tooltip="This specification extends the Web Distributed Authoring and Versioning Protocol (WebDAV) to support datatyping.  Protocol elements are defined to let clients and servers specify the datatype, and to instruct the WebDAV method PROPFIND to return datatype information.  This memo defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
