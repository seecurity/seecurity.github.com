digraph Flow {
  layout=twopi;
  root=RFC3832;
  overlap=false;
  RFC3832 [label="RFC3832
Remote Service Discovery in the Service
Location Protocol (SLP) via DNS SRV
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="Remote service discovery refers to discovering desired services in given remote (i.e., non-local) DNS domains.  This document describes remote service discovery in the Service Location Protocol (SLP) via DNS SRV.  It defines the DNS SRV Resource Records for SLP Directory Agent services, discusses various issues in using SLP and DNS SRV together for remote service discovery, and gives the steps for discovering desired services in remote DNS domains.  This memo defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3832;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3832 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3832 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3832;
  RFC8553 [label="RFC8553
DNS Attrleaf Changes: Fixing
Specifications That Use Underscored Node
Names
2019", shape=ellipse, URL="RFC8553.html", target="_top", tooltip="Using an underscore for a prefix creates a space for constrained interoperation of resource records.  Original uses of an underscore character as a domain node name prefix were specified without the benefit of an IANA registry.  This produced an entirely uncoordinated set of name-creation activities, all drawing from the same namespace.  A registry for these names has now been defined by RFC 8552.  However, the existing specifications that use underscored naming need to be modified in order to be in line with the new registry.  This document specifies those changes.  The changes preserve existing software and operational practice, while adapting the specifications for those practices to the newer underscore registry model."];
  RFC8553 -> updated_by;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC3832 -> cites;
  RFC2608 [label="RFC2608
Service Location Protocol, Version 2
1999", shape=ellipse, URL="RFC2608.html", target="_top", tooltip="The Service Location Protocol provides a scalable framework for the discovery and selection of network services.  Using this protocol, computers using the Internet need little or no static configuration of network services for network based applications.  This is especially important as computers become more portable, and users less tolerant or able to fulfill the demands of network system administration. [STANDARDS-TRACK]"];
  cites -> RFC2608;
  RFC2782 [label="RFC2782
A DNS RR for specifying the location of
services (DNS SRV)
2000", shape=ellipse, URL="RFC2782.html", target="_top", tooltip="This document describes a DNS RR which specifies the location of the server(s) for a specific protocol and domain. [STANDARDS-TRACK]"];
  cites -> RFC2782;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC1034 [label="RFC1034
Domain names - concepts and facilities
1987", shape=ellipse, URL="RFC1034.html", target="_top", tooltip="This RFC is the revised basic definition of The Domain Name System.  It obsoletes RFC-882.  This memo describes the domain style names and their used for host address look up and electronic mail forwarding.  It discusses the clients and servers in the domain name system and the protocol used between them."];
  cites -> RFC1034;
  RFC1035 [label="RFC1035
Domain names - implementation and
specification
1987", shape=ellipse, URL="RFC1035.html", target="_top", tooltip="This RFC is the revised specification of the protocol and format used in the implementation of the Domain Name System.  It obsoletes RFC-883.  This memo documents the details of the domain name client - server communication."];
  cites -> RFC1035;
  RFC2535 [label="RFC2535
Domain Name System Security Extensions
1999", shape=ellipse, URL="RFC2535.html", target="_top", tooltip="This document incorporates feedback on RFC 2065 from early implementers and potential users. [STANDARDS-TRACK]"];
  cites -> RFC2535;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC3832;
  RFC5505 [label="RFC5505
Principles of Internet Host
Configuration
2009", shape=ellipse, URL="RFC5505.html", target="_top", tooltip="This document describes principles of Internet host configuration.  It covers issues relating to configuration of Internet-layer parameters, as well as parameters affecting higher-layer protocols.  This memo provides information for the Internet community."];
  RFC5505 -> rev_cites;
  RFC5887 [label="RFC5887
Renumbering Still Needs Work
2010", shape=ellipse, URL="RFC5887.html", target="_top", tooltip="This document reviews the existing mechanisms for site renumbering for both IPv4 and IPv6, and it identifies operational issues with those mechanisms.  It also summarises current technical proposals for additional mechanisms.  Finally, there is a gap analysis identifying possible areas for future work.  This document is not an Internet Standards Track specification; it is published for informational purposes."];
  RFC5887 -> rev_cites;
  RFC8553 [label="RFC8553
DNS Attrleaf Changes: Fixing
Specifications That Use Underscored Node
Names
2019", shape=ellipse, URL="RFC8553.html", target="_top", tooltip="Using an underscore for a prefix creates a space for constrained interoperation of resource records.  Original uses of an underscore character as a domain node name prefix were specified without the benefit of an IANA registry.  This produced an entirely uncoordinated set of name-creation activities, all drawing from the same namespace.  A registry for these names has now been defined by RFC 8552.  However, the existing specifications that use underscored naming need to be modified in order to be in line with the new registry.  This document specifies those changes.  The changes preserve existing software and operational practice, while adapting the specifications for those practices to the newer underscore registry model."];
  RFC8553 -> rev_cites;
}
