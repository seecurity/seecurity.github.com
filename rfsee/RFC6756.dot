digraph Flow {
  layout=twopi;
  root=RFC6756;
  overlap=false;
  RFC6756 [label="RFC6756
Internet Engineering Task Force and
International Telecommunication Union -
Telecommunication Standardization Sector
Collaboration Guidelines
2012", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document provides guidance to aid in the understanding of collaboration on standards development between the Telecommunication Standardization Sector of the International Telecommunication Union (ITU-T) and the Internet Engineering Task Force (IETF) of the Internet Society (ISOC). It is an update of and obsoletes RFC 3356. The updates reflect changes in the IETF and ITU-T since RFC 3356 was written. The bulk of this document is common text with ITU-T A Series Supplement 3 (07/2012). Note: This was approved by TSAG on 4 July 2012 as Supplement 3 to the ITU-T A-Series of Recommendations. This document is not an Internet Standards Track specification; it is published for informational purposes.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC6756;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC6756 -> obs;
  RFC3356 [label="RFC3356
Internet Engineering Task Force and
International Telecommunication Union -
Telecommunications Standardization
Sector Collaboration Guidelines
2002", shape=ellipse, URL="RFC3356.html", target="_top", tooltip="None"];
  obs -> RFC3356;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC6756 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC6756;
  RFC9141 [label="RFC9141
Updating References to the IETF FTP
Service
2021", shape=ellipse, URL="RFC9141.html", target="_top", tooltip="The IETF FTP service running at ftp.ietf.org, ops.ietf.org, and ietf.org will be retired.  A number of published RFCs in the IETF and IAB streams include URIs that reference this FTP service.  To ensure that the materials referenced using the IETF FTP service can still be found, this document updates the FTP-based references in these affected documents with HTTPS URIs."];
  RFC9141 -> updated_by;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC6756 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC6756;
  RFC7241 [label="RFC7241
The IEEE 802/IETF Relationship
2014", shape=ellipse, URL="RFC7241.html", target="_top", tooltip="This document describes the standardization cooperation between Project 802 of the Institute of Electrical and Electronics Engineers (IEEE) and the Internet Engineering Task Force (IETF). This document obsoletes RFC 4441. Note: This document was collaboratively developed by authors from both the IEEE 802 and IETF leadership and was reviewed and approved by the IEEE 802 Executive Committee prior to publication."];
  RFC7241 -> rev_cites;
  RFC8712 [label="RFC8712
The IETF-ISOC Relationship
2020", shape=ellipse, URL="RFC8712.html", target="_top", tooltip="This document summarizes the Internet Engineering Task Force (IETF) - Internet Society (ISOC) relationship, following a major revision to the structure of the IETF Administrative Support Activity (IASA) in 2018.  The IASA was revised under a new IASA 2.0 structure by the IASA2 Working Group, which changed the IETF's administrative, legal, and financial structure.  As a result, it also changed the relationship between the IETF and ISOC, which made it necessary to revise RFC 2031."];
  RFC8712 -> rev_cites;
  RFC9141 [label="RFC9141
Updating References to the IETF FTP
Service
2021", shape=ellipse, URL="RFC9141.html", target="_top", tooltip="The IETF FTP service running at ftp.ietf.org, ops.ietf.org, and ietf.org will be retired.  A number of published RFCs in the IETF and IAB streams include URIs that reference this FTP service.  To ensure that the materials referenced using the IETF FTP service can still be found, this document updates the FTP-based references in these affected documents with HTTPS URIs."];
  RFC9141 -> rev_cites;
}
