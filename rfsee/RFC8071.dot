  RFC8071 [label="RFC8071
NETCONF Call Home and RESTCONF Call Home
2017", shape=ellipse, fillcolor="#e8f0ff", tooltip="This RFC presents NETCONF Call Home and RESTCONF Call Home, which enable a NETCONF or RESTCONF server to initiate a secure connection to a NETCONF or RESTCONF client, respectively.", style=filled, fillcolor=green]; 

