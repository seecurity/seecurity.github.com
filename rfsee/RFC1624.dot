  RFC1141 [label="RFC1141
Incremental updating of the Internet
checksum
1990", shape=ellipse, URL="RFC1141.html", target="_top", tooltip="This memo correctly describes the incremental update procedure for use with the standard Internet checksum.  It is intended to replace the description of Incremental Update in RFC 1071.  This is not a standard but rather, an implementation technique."];
  updates -> RFC1141;

  updated_by [label="updates", shape=box];
  updated_by -> RFC1624;

