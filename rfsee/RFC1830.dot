digraph Flow {
  layout=twopi;
  root=RFC1830;
  overlap=false;
  RFC1830 [label="RFC1830
SMTP Service Extensions for Transmission
of Large and Binary MIME Messages
1995", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo defines two extensions to the SMTP service.  The first service enables a SMTP client and server to negotiate the use of an alternate DATA command BDAT for efficiently sending large MIME messages.  The second extension takes advantage of the BDAT command to permit the negotiated sending of unencoded binary data.  This memo defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1830;
  RFC3030 [label="RFC3030
SMTP Service Extensions for Transmission
of Large and Binary MIME Messages
2000", shape=ellipse, URL="RFC3030.html", target="_top", tooltip="This memo defines two extensions to the SMTP (Simple Mail Transfer Protocol) service. [STANDARDS-TRACK]"];
  RFC3030 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1830 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC1830 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC1830;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC1830 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC1830;
}
