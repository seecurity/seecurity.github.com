  RFC3740 [label="RFC3740
The Multicast Group Security
Architecture
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document provides an overview and rationale of the multicast security architecture used to secure data packets of large multicast groups.  The document begins by introducing a Multicast Security Reference Framework, and proceeds to identify the security services that may be part of a secure multicast solution.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3740;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
