  RFC2224 [label="RFC2224
NFS URL Scheme
1997", shape=ellipse, fillcolor="#e8f0ff", tooltip="A new URL scheme, 'nfs' is defined.  It is used to refer to files and directories on NFS servers using the general URL syntax defined in RFC 1738, Uniform Resource Locators (URL).  This memo provides information for the Internet community.  It does not specify an Internet standard of any kind."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC2224;

  obs [label="obsoletes", shape=box];
  RFC2224 -> obs;

  updates [label="updates", shape=box];
  RFC2224 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC2224;

  cites [label="cites", shape=box];
  RFC2224 -> cites;
  RFC2054 [label="RFC2054
WebNFS Client Specification
1996", shape=ellipse, URL="RFC2054.html", target="_top", tooltip="This document describes a lightweight binding mechanism that allows NFS clients to obtain service from WebNFS-enabled servers with a minimum of protocol overhead.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC2054;
  RFC2055 [label="RFC2055
WebNFS Server Specification
1996", shape=ellipse, URL="RFC2055.html", target="_top", tooltip="This document describes the specifications for a server of WebNFS clients.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC2055;
  RFC1094 [label="RFC1094
NFS: Network File System Protocol
specification
1989", shape=ellipse, URL="RFC1094.html", target="_top", tooltip="This RFC describes a protocol that Sun Microsystems, Inc., and others are using.  A new version of the protocol is under development, but others may benefit from the descriptions of the current protocol, and discussion of some of the design issues."];
  cites -> RFC1094;
  RFC1813 [label="RFC1813
NFS Version 3 Protocol Specification
1995", shape=ellipse, URL="RFC1813.html", target="_top", tooltip="This paper describes the NFS version 3 protocol.  This paper is provided so that people can write compatible implementations.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  cites -> RFC1813;
  RFC1831 [label="RFC1831
RPC: Remote Procedure Call Protocol
Specification Version 2
1995", shape=ellipse, URL="RFC1831.html", target="_top", tooltip="This document describes the ONC Remote Procedure Call (ONC RPC Version 2) protocol as it is currently deployed and accepted. [STANDARDS-TRACK]"];
  cites -> RFC1831;
  RFC1832 [label="RFC1832
