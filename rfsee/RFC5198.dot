  RFC8766 -> rev_cites;
  RFC8819 [label="RFC8819
YANG Module Tags
2021", shape=ellipse, URL="RFC8819.html", target="_top", tooltip="This document provides for the association of tags with YANG modules.  The expectation is for such tags to be used to help classify and organize modules.  A method for defining, reading, and writing modules tags is provided.  Tags may be registered and assigned during module definition, assigned by implementations, or dynamically defined and set by users.  This document also provides guidance to future model writers; as such, this document updates RFC 8407."];
