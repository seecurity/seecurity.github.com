digraph Flow {
  rankdir=LR;
  splines=true;
  concentrate=true;   // merge parallel edges / reduce clutter
  nodesep=0.25;       // horizontal/vertical separation between nodes
  ranksep=0.35;       // separation between ranks (columns in LR)
  RFC1642 [label="RFC1642
UTF-7 - A Mail-Safe Transformation
Format of Unicode
1994", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document describes a new transformation format of Unicode that contains only 7-bit ASCII characters and is intended to be readable by humans in the limiting case that the document consists of characters from the US-ASCII repertoire.  This memo defines an Experimental Protocol for the Internet community.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1642;
  RFC2152 [label="RFC2152
UTF-7 A Mail-Safe Transformation Format
of Unicode
1997", shape=ellipse, URL="RFC2152.html", target="_top", tooltip="This document describes a transformation format of Unicode that contains only 7-bit ASCII octets and is intended to be readable by humans in the limiting case that the document consists of characters from the US-ASCII repertoire.  It also specifies how this transformation format is used in the context of MIME and RFC 1641, Using Unicode with MIME.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  RFC2152 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1642 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC1642 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC1642;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC1642 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC1642;
  RFC1700 [label="RFC1700
Assigned Numbers
1994", shape=ellipse, URL="RFC1700.html", target="_top", tooltip="This RFC is a snapshot of the ongoing process of the assignment of protocol parameters for the Internet protocol suite.  To make the current information readily available the assignments are kept up-to- date in a set of online text files.  This memo is a status report on the parameters (i.e., numbers and keywords) used in protocols in the Internet community."];
  RFC1700 -> rev_cites;
  RFC2044 [label="RFC2044
UTF-8, a transformation format of
Unicode and ISO 10646
1996", shape=ellipse, URL="RFC2044.html", target="_top", tooltip="The Unicode Standard, version 1.1, and ISO/IEC 10646-1:1993 jointly define a 16 bit character set which encompasses most of the world's writing systems.  UTF-8, the object of this memo, has the characteristic of preserving the full US-ASCII range.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  RFC2044 -> rev_cites;
  RFC2070 [label="RFC2070
Internationalization of the Hypertext
Markup Language
1997", shape=ellipse, URL="RFC2070.html", target="_top", tooltip="This document is meant to address the issue of the internationalization (i18n, i followed by 18 letters followed by n) of HTML by extending the specification of HTML and giving additional recommendations for proper internationalization support. [STANDARDS-TRACK]"];
  RFC2070 -> rev_cites;
}
