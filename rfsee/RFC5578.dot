digraph Flow {
  layout=twopi;
  root=RFC5578;
  overlap=false;
  RFC5578 [label="RFC5578
PPP over Ethernet (PPPoE) Extensions for
Credit Flow and Link Metrics
2010", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document extends the Point-to-Point Protocol over Ethernet (PPPoE) with an optional credit-based flow control mechanism and an optional Link Quality Metric report.  These optional extensions improve the performance of PPPoE over media with variable bandwidth and limited buffering, such as mobile point-to-point radio links.  This document is not an Internet Standards Track specification; it is published for informational purposes.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC5578;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC5578 -> obs;
  RFC4938 [label="RFC4938
PPP Over Ethernet (PPPoE) Extensions for
Credit Flow and Link Metrics
2007", shape=ellipse, URL="RFC4938.html", target="_top", tooltip="This document extends the Point-to-Point over Ethernet (PPPoE) Protocol with a credit-based flow control mechanism and Link Quality Metric report.  This optional extension should improve the performance of PPPoE over media with variable bandwidth and limited buffering, such as mobile radio links.  This memo provides information for the Internet community."];
  obs -> RFC4938;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC5578 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC5578;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC5578 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC5578;
  RFC8651 [label="RFC8651
Dynamic Link Exchange Protocol (DLEP)
Control-Plane-Based Pause Extension
2019", shape=ellipse, URL="RFC8651.html", target="_top", tooltip="This document defines an extension to the Dynamic Link Exchange Protocol (DLEP) that enables a modem to use DLEP messages to pause and resume data traffic coming from its peer router."];
  RFC8651 -> rev_cites;
}
