digraph Flow {
  rankdir=LR;
  splines=true;
  concentrate=true;   // merge parallel edges / reduce clutter
  nodesep=0.25;       // horizontal/vertical separation between nodes
  ranksep=0.35;       // separation between ranks (columns in LR)
  RFC1888 [label="RFC1888
OSI NSAPs and IPv6
1996", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document recommends that network implementors who have planned or deployed an OSI NSAP addressing plan, and who wish to deploy or transition to IPv6, should redesign a native IPv6 addressing plan to meet their needs.  This memo defines an Experimental Protocol for the Internet community."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC1888;
  RFC4048 [label="RFC4048
RFC 1888 Is Obsolete
2005", shape=ellipse, URL="RFC4048.html", target="_top", tooltip="This document recommends that RFC 1888, on Open Systems Interconnection (OSI) Network Service Access Points (NSAPs) and IPv6, be reclassified as Historic, as most of it has no further value, apart from one section, which is faulty.  This memo provides information for the Internet community."];
  RFC4048 -> obs_by;

  obs [label="obsoletes", shape=box];
  RFC1888 -> obs;

  updates [label="updates", shape=box];
  RFC1888 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC1888;
  RFC4548 [label="RFC4548
Internet Code Point (ICP) Assignments
for NSAP Addresses
2006", shape=ellipse, URL="RFC4548.html", target="_top", tooltip="This document is intended to accomplish two highly inter-related tasks: to establish an initial Internet Code Point (ICP) assignment for each of IPv4 and IPv6 address encoding in Network Service Access Point (NSAP) Addresses, and to recommend an IANA assignment policy for currently unassigned ICP values.  In the first task, this document is a partial replacement for RFC 1888 -- particularly for section 6 of RFC 1888.  In the second task, this document incorporates wording and specifications from ITU-T Recommendation X.213 and further recommends that IANA use the IETF consensus assignment policy in making future ICP assignments. [STANDARDS-TRACK]"];
  RFC4548 -> updated_by;

  cites [label="cites", shape=box];
  RFC1888 -> cites;
  RFC1883 [label="RFC1883
Internet Protocol, Version 6 (IPv6)
Specification
1995", shape=ellipse, URL="RFC1883.html", target="_top", tooltip="This document specifies version 6 of the Internet Protocol (IPv6), also sometimes referred to as IP Next Generation or IPng. [STANDARDS-TRACK]"];
  cites -> RFC1883;
  RFC1629 [label="RFC1629
Guidelines for OSI NSAP Allocation in
the Internet
1994", shape=ellipse, URL="RFC1629.html", target="_top", tooltip="This paper provides guidelines for allocating NSAP addresses in the Internet.  The guidelines provided in this paper have been the basis for initial deployment of CLNP in the Internet, and have proven very valuable both as an aid to scaling of CLNP routing, and for address administration. [STANDARDS-TRACK]"];
  cites -> RFC1629;
  RFC1884 [label="RFC1884
IP Version 6 Addressing Architecture
1995", shape=ellipse, URL="RFC1884.html", target="_top", tooltip="This specification defines the addressing architecture of the IP Version 6 protocol [IPV6]. [STANDARDS-TRACK]"];
  cites -> RFC1884;
  RFC1971 [label="RFC1971
IPv6 Stateless Address Autoconfiguration
1996", shape=ellipse, URL="RFC1971.html", target="_top", tooltip="This document specifies the steps a host takes in deciding how to autoconfigure its interfaces in IP version 6. [STANDARDS-TRACK]"];
  cites -> RFC1971;
  RFC1970 [label="RFC1970
Neighbor Discovery for IP Version 6
(IPv6)
1996", shape=ellipse, URL="RFC1970.html", target="_top", tooltip="This document specifies the Neighbor Discovery protocol for IP Version 6. [STANDARDS-TRACK]"];
  cites -> RFC1970;
  RFC1326 [label="RFC1326
Mutual Encapsulation Considered
Dangerous
1992", shape=ellipse, URL="RFC1326.html", target="_top", tooltip="This memo describes a packet explosion problem that can occur with mutual encapsulation of protocols (A encapsulates B and B encapsulates A).  This memo provides information for the Internet community.  It does not specify an Internet standard."];
  cites -> RFC1326;
  RFC1825 [label="RFC1825
Security Architecture for the Internet
Protocol
1995", shape=ellipse, URL="RFC1825.html", target="_top", tooltip="This memo describes the security mechanisms for IP version 4 (IPv4) and IP version 6 (IPv6) and the services that they provide. [STANDARDS-TRACK]"];
  cites -> RFC1825;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC1888;
  RFC3513 [label="RFC3513
Internet Protocol Version 6 (IPv6)
Addressing Architecture
2003", shape=ellipse, URL="RFC3513.html", target="_top", tooltip="This specification defines the addressing architecture of the IP Version 6 (IPv6) protocol.  The document includes the IPv6 addressing model, text representations of IPv6 addresses, definition of IPv6 unicast addresses, anycast addresses, and multicast addresses, and an IPv6 node's required addresses. [STANDARDS-TRACK]"];
  RFC3513 -> rev_cites;
  RFC4048 [label="RFC4048
RFC 1888 Is Obsolete
2005", shape=ellipse, URL="RFC4048.html", target="_top", tooltip="This document recommends that RFC 1888, on Open Systems Interconnection (OSI) Network Service Access Points (NSAPs) and IPv6, be reclassified as Historic, as most of it has no further value, apart from one section, which is faulty.  This memo provides information for the Internet community."];
  RFC4048 -> rev_cites;
  RFC4147 [label="RFC4147
Proposed Changes to the Format of the
IANA IPv6 Registry
2005", shape=ellipse, URL="RFC4147.html", target="_top", tooltip="This document proposes a revised format for the IANA IPv6 address registries.  Rather than providing a formal definition of the format, it is described by giving examples of the (current as of preparation of this document) contents of the registries in the proposed format.  The proposed format would bring the IANA IPv6 address registries into alignment with the current IPv6 Address Architecture specification, as well as update it to a more useful and generally accepted format.  This memo provides information for the Internet community."];
  RFC4147 -> rev_cites;
}
