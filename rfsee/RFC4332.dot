  RFC3012 [label="RFC3012
Mobile IPv4 Challenge/Response
Extensions
2000", shape=ellipse, URL="RFC3012.html", target="_top", tooltip="In this specification, we define extensions for the Mobile IP Agent Advertisements and the Registration Request that allow a foreign agent to use a challenge/response mechanism to authenticate the mobile node. [STANDARDS-TRACK]"];
  cites -> RFC3012;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
