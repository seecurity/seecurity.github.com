  RFC2131 [label="RFC2131
Dynamic Host Configuration Protocol
1997", shape=ellipse, URL="RFC2131.html", target="_top", tooltip="The Dynamic Host Configuration Protocol (DHCP) provides a framework for passing configuration information to hosts on a TCPIP network.  DHCP is based on the Bootstrap Protocol (BOOTP), adding the capability of automatic allocation of reusable network addresses and additional configuration options. [STANDARDS-TRACK]"];
  cites -> RFC2131;
  RFC1332 [label="RFC1332
The PPP Internet Protocol Control
Protocol (IPCP)
1992", shape=ellipse, URL="RFC1332.html", target="_top", tooltip="The Point-to-Point Protocol (PPP) [1] provides a standard method of encapsulating Network Layer protocol information over point-to-point links.  PPP also defines an extensible Link Control Protocol, and proposes a family of Network Control Protocols (NCPs) for establishing and configuring different network-layer protocols. [STANDARDS-TRACK]"];
  cites -> RFC1332;
  RFC3344 [label="RFC3344
IP Mobility Support for IPv4
2002", shape=ellipse, URL="RFC3344.html", target="_top", tooltip="None"];
