  RFC2089 [label="RFC2089
V2ToV1 Mapping SNMPv2 onto SNMPv1 within
a bi-lingual SNMP agent
1997", shape=ellipse, fillcolor="#e8f0ff", tooltip="The goal of this memo is to document a common way of mapping an SNMPv2 response into an SNMPv1 response within a bi-lingual SNMP agent (one that supports both SNMPv1 and SNMPv2).  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2089;
  RFC2576 [label="RFC2576
Coexistence between Version 1, Version
2, and Version 3 of the Internet-
standard Network Management Framework
2000", shape=ellipse, URL="RFC2576.html", target="_top", tooltip="The purpose of this document is to describe coexistence between version 3 of the Internet-standard Network Management Framework, (SNMPv3), version 2 of the Internet-standard Network Management Framework (SNMPv2), and the original Internet-standard Network Management Framework (SNMPv1). [STANDARDS-TRACK]"];
  RFC2576 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2089 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2089 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2089;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2089 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2089;
  RFC3584 [label="RFC3584
Coexistence between Version 1, Version
2, and Version 3 of the Internet-
standard Network Management Framework
2003", shape=ellipse, URL="RFC3584.html", target="_top", tooltip="The purpose of this document is to describe coexistence between version 3 of the Internet-standard Network Management Framework, (SNMPv3), version 2 of the Internet-standard Network Management Framework (SNMPv2), and the original Internet-standard Network Management Framework (SNMPv1).  This document also describes how to convert MIB modules from SMIv1 format to SMIv2 format.  This document obsoletes RFC 2576.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  RFC3584 -> rev_cites;
