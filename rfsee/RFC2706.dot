  overlap=false;
  RFC2706 [label="RFC2706
ECML v1: Field Names for E-Commerce
1999", shape=ellipse, fillcolor="#e8f0ff", tooltip="A standard set of information fields is defined as the first version of an Electronic Commerce Modeling Language (ECML) so that this task can be more easily automated, for example by wallet software that could fill in fields.  This memo provides information for the Internet community.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2706;
  RFC3106 [label="RFC3106
ECML v1.1: Field Specifications for
E-Commerce
2001", shape=ellipse, URL="RFC3106.html", target="_top", tooltip="Customers are frequently required to enter substantial amounts of information at an Internet merchant site in order to complete a purchase or other transaction, especially the first time they go there.  A standard set of information fields is defined as the first version of an Electronic Commerce Modeling Language (ECML) so that this task can be more easily automated, for example by wallet software that could fill in fields.  Even for the manual data entry case, customers will be less confused by varying merchant sites if a substantial number adopt these standard fields.  In addition, some fields are defined for merchant to consumer communication.  This memo provides information for the Internet community."];
  RFC3106 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2706 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2706 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2706;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2706 -> cites;
  RFC2246 [label="RFC2246
The TLS Protocol Version 1.0
1999", shape=ellipse, URL="RFC2246.html", target="_top", tooltip="This document specifies Version 1.0 of the Transport Layer Security (TLS) protocol.  The TLS protocol provides communications privacy over the Internet.  The protocol allows client/server applications to communicate in a way that is designed to prevent eavesdropping, tampering, or message forgery."];
  cites -> RFC2246;
  RFC2411 [label="RFC2411
IP Security Document Roadmap
1998", shape=ellipse, URL="RFC2411.html", target="_top", tooltip="This document is intended to provide guidelines for the development of collateral specifications describing the use of new encryption and authentication algorithms with the ESP protocol, described in and new authentication algorithms used with the AH protocol.  This memo provides information for the Internet community."];
  cites -> RFC2411;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2706;
  RFC3106 [label="RFC3106
ECML v1.1: Field Specifications for
E-Commerce
2001", shape=ellipse, URL="RFC3106.html", target="_top", tooltip="Customers are frequently required to enter substantial amounts of information at an Internet merchant site in order to complete a purchase or other transaction, especially the first time they go there.  A standard set of information fields is defined as the first version of an Electronic Commerce Modeling Language (ECML) so that this task can be more easily automated, for example by wallet software that could fill in fields.  Even for the manual data entry case, customers will be less confused by varying merchant sites if a substantial number adopt these standard fields.  In addition, some fields are defined for merchant to consumer communication.  This memo provides information for the Internet community."];
