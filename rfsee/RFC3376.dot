  RFC2236 [label="RFC2236
Internet Group Management Protocol,
Version 2
1997", shape=ellipse, URL="RFC2236.html", target="_top", tooltip="This memo documents IGMPv2, used by IP hosts to report their multicast group memberships to routers.  It updates STD 5, RFC 1112. [STANDARDS-TRACK]"];
  updates -> RFC2236;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
