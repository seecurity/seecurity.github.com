  RFC3735 [label="RFC3735
Guidelines for Extending the Extensible
Provisioning Protocol (EPP)
2004", shape=ellipse, fillcolor="#e8f0ff", tooltip="The Extensible Provisioning Protocol (EPP) is an application layer client-server protocol for the provisioning and management of objects stored in a shared central repository.  Specified in XML, the protocol defines generic object management operations and an extensible framework that maps protocol operations to objects.  This document presents guidelines for use of EPP's extension mechanisms to define new features and object management capabilities.  This memo provides information for the Internet community.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC3735;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC3735 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC3735 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC3735;

