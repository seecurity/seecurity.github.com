  updated_by [label="updates", shape=box];
  updated_by -> RFC7632;

  cites [label="cites", shape=box];
  RFC7632 -> cites;
  RFC3444 [label="RFC3444
On the Difference between Information
Models and Data Models
2003", shape=ellipse, URL="RFC3444.html", target="_top", tooltip="There has been ongoing confusion about the differences between Information Models and Data Models for defining managed objects in network management.  This document explains the differences between these terms by analyzing how existing network management model specifications (from the IETF and other bodies such as the International Telecommunication Union (ITU) or the Distributed Management Task Force (DMTF)) fit into the universe of Information Models and Data Models.  This memo documents the main results of the 8th workshop of the Network Management Research Group (NMRG) of the Internet Research Task Force (IRTF) hosted by the University of Texas at Austin.  This memo provides information for the Internet community."];
  cites -> RFC3444;
  RFC3954 [label="RFC3954
Cisco Systems NetFlow Services Export
Version 9
2004", shape=ellipse, URL="RFC3954.html", target="_top", tooltip="This document specifies the data export format for version 9 of Cisco Systems' NetFlow services, for use by implementations on the network elements and/or matching collector programs.  The version 9 export format uses templates to provide access to observations of IP packet flows in a flexible and extensible manner.  A template defines a collection of fields, with corresponding descriptions of structure and semantics.  This memo provides information for the Internet community."];
  cites -> RFC3954;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC7632;
  RFC8248 [label="RFC8248
Security Automation and Continuous
Monitoring (SACM) Requirements
2017", shape=ellipse, URL="RFC8248.html", target="_top", tooltip="This document defines the scope and set of requirements for the Security Automation and Continuous Monitoring (SACM) architecture, data model, and transfer protocols.  The requirements and scope are based on the agreed-upon use cases described in RFC 7632."];
  RFC8248 -> rev_cites;
  RFC8412 [label="RFC8412
Software Inventory Message and
Attributes (SWIMA) for PA-TNC
2018", shape=ellipse, URL="RFC8412.html", target="_top", tooltip="This document extends PA-TNC: A Posture Attribute (PA) Protocol Compatible with Trusted Network Connect (TNC) (RFC 5792) by providing specific attributes and message exchanges to allow endpoints to report their installed software inventory information to a NEA Server, as defined in Network Endpoint Assessment (NEA): Overview and Requirements (RFC 5209)."];
  RFC8412 -> rev_cites;
