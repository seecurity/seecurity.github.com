  RFC9493 [label="RFC9493
Subject Identifiers for Security Event
Tokens
2023", shape=ellipse, fillcolor="#e8f0ff", tooltip="Security events communicated within Security Event Tokens may support a variety of identifiers to identify subjects related to the event.  This specification formalizes the notion of Subject Identifiers as structured information that describes a subject and named formats that define the syntax and semantics for encoding Subject Identifiers as JSON objects.  It also establishes a registry for defining and allocating names for such formats as well as the JSON Web Token (JWT) sub_id Claim.", style=filled, fillcolor=green]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC9493;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC9493 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
