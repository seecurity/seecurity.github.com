  RFC8343 [label="RFC8343
A YANG Data Model for Interface
Management
2018", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines a YANG data model for the management of network interfaces. It is expected that interface-type-specific data models augment the generic interfaces data model defined in this document. The data model includes definitions for configuration and system state (status information and counters for the collection of statistics). The YANG data model in this document conforms to the Network Management Datastore Architecture (NMDA) defined in RFC 8342. This document obsoletes RFC 7223.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC8343;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC8343 -> obs;
  RFC7223 [label="RFC7223
A YANG Data Model for Interface
Management
2014", shape=ellipse, URL="RFC7223.html", target="_top", tooltip="This document defines a YANG data model for the management of network interfaces.  It is expected that interface-type-specific data models augment the generic interfaces data model defined in this document.  The data model includes configuration data and state data (status information and counters for the collection of statistics)."];
  obs -> RFC7223;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC8343 -> updates;

