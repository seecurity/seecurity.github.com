  RFC2965 [label="RFC2965
HTTP State Management Mechanism
2000", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document specifies a way to create a stateful session with Hypertext Transfer Protocol (HTTP) requests and responses. [STANDARDS-TRACK]"]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC2965;
  RFC6265 [label="RFC6265
HTTP State Management Mechanism
2011", shape=ellipse, URL="RFC6265.html", target="_top", tooltip="This document defines the HTTP Cookie and Set-Cookie header fields.  These header fields can be used by HTTP servers to store state (called cookies) at HTTP user agents, letting the servers maintain a stateful session over the mostly stateless HTTP protocol.  Although cookies have many historical infelicities that degrade their security and privacy, the Cookie and Set-Cookie header fields are widely used on the Internet.  This document obsoletes RFC 2965. [STANDARDS-TRACK]"];
  RFC6265 -> obs_by;

  obs [label="obsoletes", shape=box];
  RFC2965 -> obs;
  RFC2109 [label="RFC2109
HTTP State Management Mechanism
1997", shape=ellipse, URL="RFC2109.html", target="_top", tooltip="This document specifies a way to create a stateful session with HTTP requests and responses.  It describes two new headers, Cookie and Set- Cookie, which carry state information between participating origin servers and user agents.  The method described here differs from Netscape's Cookie proposal, but it can interoperate with HTTP/1.0 user agents that use Netscape's method. [STANDARDS-TRACK]"];
  obs -> RFC2109;

  updates [label="updates", shape=box];
  RFC2965 -> updates;

