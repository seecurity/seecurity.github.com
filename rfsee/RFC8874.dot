  RFC2418 [label="RFC2418
IETF Working Group Guidelines and
Procedures
1998", shape=ellipse, URL="RFC2418.html", target="_top", tooltip="This document describes the guidelines and procedures for formation and operation of IETF working groups.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2418;
  RFC8875 [label="RFC8875
Working Group GitHub Administration
2020", shape=ellipse, URL="RFC8875.html", target="_top", tooltip="The use of GitHub in IETF working group processes is increasing.  This document describes uses and conventions for working groups that are considering starting to use GitHub.  It does not mandate any processes and does not require changes to the processes used by current and future working groups not using GitHub."];
