  RFC2447 [label="RFC2447
iCalendar Message-Based Interoperability
Protocol (iMIP)
1998", shape=ellipse, URL="RFC2447.html", target="_top", tooltip="This document specifies a binding from the iCalendar Transport- independent Interoperability Protocol (iTIP) to Internet email-based transports. [STANDARDS-TRACK]"];
  obs -> RFC2447;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
