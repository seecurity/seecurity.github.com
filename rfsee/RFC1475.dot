  RFC1475 [label="RFC1475
TP/IX: The Next Internet
1993", shape=ellipse, fillcolor="#e8f0ff", tooltip="This memo presents the specification for version 7 of the Internet Protocol, as well as version 7 of the TCP and the user datagram protocol.  This memo defines an Experimental Protocol for the Internet community.  It does not specify an Internet standard.", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC1475;
  RFC6814 [label="RFC6814
Formally Deprecating Some IPv4 Options
2012", shape=ellipse, URL="RFC6814.html", target="_top", tooltip="A number of IPv4 options have become obsolete in practice, but have never been formally deprecated.  This document deprecates such IPv4 options, thus cleaning up the corresponding IANA registry.  Additionally, it obsoletes RFCs 1385, 1393, 1475, and 1770, and requests that the RFC Editor change their status to Historic. [STANDARDS-TRACK]"];
  RFC6814 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC1475 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
