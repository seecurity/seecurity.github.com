  RFC2052 [label="RFC2052
A DNS RR for specifying the location of
services (DNS SRV)
1996", shape=ellipse, URL="RFC2052.html", target="_top", tooltip="This document describes a DNS RR which specifies the location of the server(s) for a specific protocol and domain (like a more general form of MX).  This memo defines an Experimental Protocol for the Internet community."];
  obs -> RFC2052;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
