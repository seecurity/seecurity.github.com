  RFC7751 [label="RFC7751
Kerberos Authorization Data Container
Authenticated by Multiple Message
Authentication Codes (MACs)
2016", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document specifies a Kerberos authorization data container that supersedes AD-KDC-ISSUED.  It allows for multiple Message Authentication Codes (MACs) or signatures to authenticate the contained authorization data elements.  The multiple MACs are needed to mitigate shortcomings in the existing AD-KDC-ISSUED container.  This document updates RFC 4120."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC7751;

  obs [label="obsoletes", shape=box];
  RFC7751 -> obs;

  updates [label="updates", shape=box];
  RFC7751 -> updates;
  RFC4120 [label="RFC4120
The Kerberos Network Authentication
Service (V5)
2005", shape=ellipse, URL="RFC4120.html", target="_top", tooltip="This document provides an overview and specification of Version 5 of the Kerberos protocol, and it obsoletes RFC 1510 to clarify aspects of the protocol and its intended use that require more detailed or clearer explanation than was provided in RFC 1510.  This document is intended to provide a detailed description of the protocol, suitable for implementation, together with descriptions of the appropriate use of protocol messages and fields within those messages. [STANDARDS-TRACK]"];
  updates -> RFC4120;

  updated_by [label="updates", shape=box];
  updated_by -> RFC7751;

  cites [label="cites", shape=box];
  RFC7751 -> cites;
  RFC4120 [label="RFC4120
The Kerberos Network Authentication
Service (V5)
2005", shape=ellipse, URL="RFC4120.html", target="_top", tooltip="This document provides an overview and specification of Version 5 of the Kerberos protocol, and it obsoletes RFC 1510 to clarify aspects of the protocol and its intended use that require more detailed or clearer explanation than was provided in RFC 1510.  This document is intended to provide a detailed description of the protocol, suitable for implementation, together with descriptions of the appropriate use of protocol messages and fields within those messages. [STANDARDS-TRACK]"];
  cites -> RFC4120;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC3961 [label="RFC3961
Encryption and Checksum Specifications
for Kerberos 5
2005", shape=ellipse, URL="RFC3961.html", target="_top", tooltip="This document describes a framework for defining encryption and checksum mechanisms for use with the Kerberos protocol, defining an abstraction layer between the Kerberos protocol and related protocols, and the actual mechanisms themselves.  The document also defines several mechanisms.  Some are taken from RFC 1510, modified in form to fit this new framework and occasionally modified in content when the old specification was incorrect.  New mechanisms are presented here as well.  This document does NOT indicate which mechanisms may be considered required to implement. [STANDARDS-TRACK]"];
  cites -> RFC3961;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC7751;
  RFC8129 [label="RFC8129
Authentication Indicator in Kerberos
Tickets
2017", shape=ellipse, URL="RFC8129.html", target="_top", tooltip="This document updates RFC 4120, as it specifies an extension in the Kerberos protocol.  It defines a new authorization data type, AD-AUTHENTICATION-INDICATOR.  The purpose of introducing this data type is to include an indicator of the strength of a client's authentication in service tickets so that application services can use it as an input into policy decisions."];
  RFC8129 -> rev_cites;
}
