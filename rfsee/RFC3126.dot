  RFC3126 [label="RFC3126
Electronic Signature Formats for long
term electronic signatures
2001", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document defines the format of an electronic signature that can remain valid over long periods.  This includes evidence as to its validity even if the signer or verifying party later attempts to deny (i.e., repudiates the validity of the signature).  This memo provides information for the Internet community."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC3126;
  RFC5126 [label="RFC5126
CMS Advanced Electronic Signatures
(CAdES)
2008", shape=ellipse, URL="RFC5126.html", target="_top", tooltip="This document defines the format of an electronic signature that can remain valid over long periods. This includes evidence as to its validity even if the signer or verifying party later attempts to deny (i.e., repudiates) the validity of the signature. The format can be considered as an extension to RFC 3852 and RFC 2634, where, when appropriate, additional signed and unsigned attributes have been defined. The contents of this Informational RFC amount to a transposition of the ETSI Technical Specification (TS) 101 733 V.1.7.4 (CMS Advanced Electronic Signatures -- CAdES) and is technically equivalent to it. The technical contents of this specification are maintained by ETSI. The ETSI TS and further updates are available free of charge at: http://www.etsi.org/WebSite/Standards/StandardsDownload.aspx This memo provides information for the Internet community."];
  RFC5126 -> obs_by;

  obs [label="obsoletes", shape=box];
  RFC3126 -> obs;

  updates [label="updates", shape=box];
  RFC3126 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC3126;

  cites [label="cites", shape=box];
  RFC3126 -> cites;
  RFC2119 [label="RFC2119
Key words for use in RFCs to Indicate
Requirement Levels
1997", shape=ellipse, URL="RFC2119.html", target="_top", tooltip="In many standards track documents several words are used to signify the requirements in the specification.  These words are often capitalized.  This document defines these words as they should be interpreted in IETF documents.  This document specifies an Internet Best Current Practices for the Internet Community, and requests discussion and suggestions for improvements."];
  cites -> RFC2119;
  RFC2459 [label="RFC2459
Internet X.509 Public Key Infrastructure
Certificate and CRL Profile
1999", shape=ellipse, URL="RFC2459.html", target="_top", tooltip="This memo profiles the X.509 v3 certificate and X.509 v2 CRL for use in the Internet. [STANDARDS-TRACK]"];
  cites -> RFC2459;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC3126;
  RFC3628 [label="RFC3628
Policy Requirements for Time-Stamping
Authorities (TSAs)
2003", shape=ellipse, URL="RFC3628.html", target="_top", tooltip="This document defines requirements for a baseline time-stamp policy for Time-Stamping Authorities (TSAs) issuing time-stamp tokens, supported by public key certificates, with an accuracy of one second or better.  A TSA may define its own policy which enhances the policy defined in this document.  Such a policy shall incorporate or further constrain the requirements identified in this document."];
  RFC3628 -> rev_cites;
  RFC4998 [label="RFC4998
Evidence Record Syntax (ERS)
2007", shape=ellipse, URL="RFC4998.html", target="_top", tooltip="In many scenarios, users must be able prove the existence and integrity of data, including digitally signed data, in a common and reproducible way over a long and possibly undetermined period of time.  This document specifies the syntax and processing of an Evidence Record, a structure designed to support long-term non-repudiation of existence of data. [STANDARDS-TRACK]"];
  RFC4998 -> rev_cites;
  RFC7107 [label="RFC7107
Object Identifier Registry for the
S/MIME Mail Security Working Group
2014", shape=ellipse, URL="RFC7107.html", target="_top", tooltip="When the S/MIME Mail Security Working Group was chartered, an object identifier arc was donated by RSA Data Security for use by that working group.  This document describes the object identifiers that were assigned in that donated arc, transfers control of that arc to IANA, and establishes IANA allocation policies for any future assignments within that arc."];
  RFC7107 -> rev_cites;
