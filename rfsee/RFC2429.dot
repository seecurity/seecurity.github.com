  RFC2429 [label="RFC2429
RTP Payload Format for the 1998 Version
of ITU-T Rec. H.263 Video (H.263+)
1998", shape=ellipse, fillcolor="#e8f0ff", tooltip="This document specifies an RTP payload header format applicable to the transmission of video streams generated based on the 1998 version of ITU-T Recommendation H.263. [STANDARDS-TRACK]", style=filled, fillcolor=green, fontsize=18, penwidth=3, fontweight=bold]; 

  obs_by [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  obs_by -> RFC2429;
  RFC4629 [label="RFC4629
RTP Payload Format for ITU-T Rec. H.263
Video
2007", shape=ellipse, URL="RFC4629.html", target="_top", tooltip="This document describes a scheme to packetize an H.263 video stream for transport using the Real-time Transport Protocol (RTP) with any of the underlying protocols that carry RTP. The document also describes the syntax and semantics of the Session Description Protocol (SDP) parameters needed to support the H.263 video codec. The document obsoletes RFC 2429 and updates the H263-1998 and H263-2000 MIME media type in RFC 3555. [STANDARDS-TRACK]"];
  RFC4629 -> obs_by;

  obs [label="obsoletes", shape=box, style=filled, fillcolor=lightblue];
  RFC2429 -> obs;

  updates [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  RFC2429 -> updates;

  updated_by [label="updates", shape=box, style=filled, fillcolor=lightyellow];
  updated_by -> RFC2429;

  cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  RFC2429 -> cites;

  rev_cites [label="cites", shape=box, style=filled, fillcolor=lightblue];
  rev_cites -> RFC2429;
  RFC3839 [label="RFC3839
MIME Type Registrations for 3rd
Generation Partnership Project (3GPP)
Multimedia files
2004", shape=ellipse, URL="RFC3839.html", target="_top", tooltip="This document serves to register and document the standard MIME types associated with the 3GPP multimedia file format, which is part of the family based on the ISO Media File Format. [STANDARDS-TRACK]"];
  RFC3839 -> rev_cites;
  RFC4393 [label="RFC4393
MIME Type Registrations for 3GPP2
Multimedia Files
2006", shape=ellipse, URL="RFC4393.html", target="_top", tooltip="This document serves to register and document the standard MIME types associated with the 3GPP2 multimedia file format, which is part of the family based on the ISO Media File Format. [STANDARDS-TRACK]"];
  RFC4393 -> rev_cites;
  RFC4628 [label="RFC4628
RTP Payload Format for H.263 Moving RFC
2190 to Historic Status
2007", shape=ellipse, URL="RFC4628.html", target="_top", tooltip="The first RFC that describes RTP payload format for ITU Telecommunication Standardization Sector (ITU-T) recommendation H.263 is RFC 2190.  This specification discusses why to move RFC 2190 to historic status.  This memo provides information for the Internet community."];
  RFC4628 -> rev_cites;
  RFC4629 [label="RFC4629
RTP Payload Format for ITU-T Rec. H.263
Video
2007", shape=ellipse, URL="RFC4629.html", target="_top", tooltip="This document describes a scheme to packetize an H.263 video stream for transport using the Real-time Transport Protocol (RTP) with any of the underlying protocols that carry RTP. The document also describes the syntax and semantics of the Session Description Protocol (SDP) parameters needed to support the H.263 video codec. The document obsoletes RFC 2429 and updates the H263-1998 and H263-2000 MIME media type in RFC 3555. [STANDARDS-TRACK]"];
  RFC4629 -> rev_cites;
