  RFC1713 [label="RFC1713
Tools for DNS debugging
1994", shape=ellipse, fillcolor="#e8f0ff", tooltip="Although widely used (and most of the times unnoticed), DNS (Domain Name System) is too much overlooked, in the sense that people, especially administrators, tend to ignore possible anomalies as long as applications that need name-to-address mapping continue to work.  This document presents some tools available for domain administrators to detect and correct those anomalies.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."]; 

  obs_by [label="obsoletes", shape=box];
  obs_by -> RFC1713;

  obs [label="obsoletes", shape=box];
  RFC1713 -> obs;

  updates [label="updates", shape=box];
  RFC1713 -> updates;

  updated_by [label="updates", shape=box];
  updated_by -> RFC1713;

  cites [label="cites", shape=box];
  RFC1713 -> cites;

  rev_cites [label="cites", shape=box];
  rev_cites -> RFC1713;
  RFC1912 [label="RFC1912
Common DNS Operational and Configuration
Errors
1996", shape=ellipse, URL="RFC1912.html", target="_top", tooltip="This memo describes errors often found in both the operation of Domain Name System (DNS) servers, and in the data that these DNS servers contain.  This memo provides information for the Internet community.  This memo does not specify an Internet standard of any kind."];
  RFC1912 -> rev_cites;
  RFC8499 [label="RFC8499
DNS Terminology
2019", shape=ellipse, URL="RFC8499.html", target="_top", tooltip="The Domain Name System (DNS) is defined in literally dozens of different RFCs. The terminology used by implementers and developers of DNS protocols, and by operators of DNS systems, has sometimes changed in the decades since the DNS was first defined. This document gives current definitions for many of the terms used in the DNS in a single document. This document obsoletes RFC 7719 and updates RFC 2308."];
  RFC8499 -> rev_cites;
  RFC9499 [label="RFC9499
DNS Terminology
2024", shape=ellipse, URL="RFC9499.html", target="_top", tooltip="The Domain Name System (DNS) is defined in literally dozens of different RFCs. The terminology used by implementers and developers of DNS protocols, and by operators of DNS systems, has changed in the decades since the DNS was first defined. This document gives current definitions for many of the terms used in the DNS in a single document. This document updates RFC 2308 by clarifying the definitions of forwarder and QNAME. It obsoletes RFC 8499 by adding multiple terms and clarifications. Comprehensive lists of changed and new definitions can be found in Appendices A and B."];
  RFC9499 -> rev_cites;
}
